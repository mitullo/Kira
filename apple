local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\197\226\124\0\138\200\254\221\112\19\135\239\226\249\107\0\129\217", "\188\150\150\25\97\230")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\252\134\83\6\9\255", "\141\186\233\63\98\108"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\194\254\41\183\41\229\226\7\191\55\240\217\56\185\55\240\237\41", "\69\145\138\76\214");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\67\219\140\136\179\2\120\235\140\136\171\30\83\199\140\138\180", "\118\16\175\233\233\223"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\175\129\52\175\230\165\114\159\129\18\174\231", "\29\235\228\85\219\142\235"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\14\192\191\220\123\90\47\117\40\221\158\216\99\75\36\70", "\50\93\180\218\189\23\46\71"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\250\161\90\88\76\242\71\202\161", "\40\190\196\59\44\36\188")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\24\64\221\160\242\83\2\40\64\251\161\243", "\109\92\37\188\212\154\29");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\52\253\161\213\52\84\16\203\161\208\37\72\11\246", "\58\100\143\196\163\81"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\46\75\55\175\58", "\110\122\34\67\195\95\41\133")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\86\190\85\94\211\123\165", "\182\21\209\59\42")]=LUAOBFUSACTOR_DECRYPT_STR_0("\132\67\192\28\45\170\191\23\238\20\51\191\247\69\192\28\37\167\246", "\222\215\55\165\125\65"),[LUAOBFUSACTOR_DECRYPT_STR_0("\8\196\212\27\230\200\226\68", "\42\76\177\166\122\146\161\141")]=3});
		end
	end));
end
setupDeathNoteStorage();
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.StealthKiraKey.Value])) then
		Options.StealthKira:SetValue(not Options.StealthKira.Value);
	end
end);
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\129\143\4\218\113\88\170\158\0\233\108\127", "\22\197\234\101\174\25"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\49\164\200\126\129\216\146\40\19\176\213", "\230\77\84\197\188\22\207\183"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\223\6\199\241\137", "\85\153\116\166\156\236\193\144")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\130\242\76\190\225", "\96\196\128\45\211\132"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\2\159\114\75\215", "\184\85\237\27\63\178\207\212"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\38\88\4\90\27", "\63\104\57\105"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\38\142\183\69", "\36\107\231\196")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\105\188\182\139\88", "\231\61\213\194")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\42\162\51\103\12\163\41", "\19\105\205\93")]=LUAOBFUSACTOR_DECRYPT_STR_0("\156\27\219\193\27\172\9\202\137\127\135\7\202\132\127\166\6\221\132\127\175\1\204\146\43\231", "\95\201\104\190\225"),[LUAOBFUSACTOR_DECRYPT_STR_0("\139\222\211\207\187\194\206\192", "\174\207\171\161")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\204\235\25\252\184\243\232\255\25\251\184\249\226\234\8\179\222\222\227\250\8\225", "\183\141\158\109\147\152"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\10\0\232\8\8\12\231\24\36\39\233\24\41", "\108\76\105\134"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\223\204\165\237\203", "\174\139\165\209\129")]=LUAOBFUSACTOR_DECRYPT_STR_0("\130\166\246\206\134\37\121\118\167\243\198\196\199\23\120\56\141\188\246\196", "\24\195\211\130\161\166\99\16"),[LUAOBFUSACTOR_DECRYPT_STR_0("\98\6\239\45\70\26\82", "\118\38\99\137\76\51")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\208\39\21", "\64\157\70\101\114\105"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\99\186\166\247\21", "\112\32\200\199\131")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\14\89\82\136\209\164\47\60\68", "\66\76\48\60\216\163\203"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\146\147\116\242\81\193\45\190\180\118\252\75\254\37\168\146", "\68\218\230\25\147\63\174"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\143\35\93\124\164\162\39\67\88", "\214\205\74\51\44"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\211\66\241\232\118\244\88\214\253\124\255\101\198\239", "\23\154\44\130\156"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\37\175\185\162\51", "\115\113\198\205\206\86")]=LUAOBFUSACTOR_DECRYPT_STR_0("\173\89\237\78\133\89\234\26\176\86\245\95\196\126\218\73", "\58\228\55\158"),[LUAOBFUSACTOR_DECRYPT_STR_0("\144\140\214\47\41\161\33", "\85\212\233\176\78\92\205")]=false});
