local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\245\205\134\223\34\254\206\242\138\204\47\217\210\214\145\223\41\239", "\138\166\185\227\190\78")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\237\123\201\51\87\49", "\121\171\20\165\87\50\67"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\245\44\188\55\181\22\206\19\176\36\184\49\210\55\171\55\190\7", "\98\166\88\217\86\217");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\197\226\124\0\138\200\254\210\124\0\146\212\213\254\124\2\141", "\188\150\150\25\97\230"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\254\140\94\22\4\195\213\157\90\37\25\228", "\141\186\233\63\98\108"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\194\254\41\183\41\229\226\11\163\44\213\239\56\179\38\229", "\69\145\138\76\214"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\84\202\136\157\183\56\127\219\140", "\118\16\175\233\233\223")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\175\129\52\175\230\165\114\159\129\18\174\231", "\29\235\228\85\219\142\235");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\13\198\191\203\114\64\51\118\56\199\174\207\120\87", "\50\93\180\218\189\23\46\71"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\234\173\79\64\65", "\40\190\196\59\44\36\188")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\31\74\210\160\255\115\25", "\109\92\37\188\212\154\29")]=LUAOBFUSACTOR_DECRYPT_STR_0("\55\251\161\194\61\78\12\175\143\202\35\91\68\253\161\194\53\67\69", "\58\100\143\196\163\81"),[LUAOBFUSACTOR_DECRYPT_STR_0("\62\87\49\162\43\64\234\0", "\110\122\34\67\195\95\41\133")]=3});
		end
	end));
end
