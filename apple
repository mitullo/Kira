local isInMatch = false;
local previousInMatch = false;
local function updateMatchState()
	local mapExists = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\17\254\243", "\88\92\159\131\164\188\195")) ~= nil;
	local inIntermission = false;
	local playerGui = LocalPlayer:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\176\34\190\82\210\249\250\149\39", "\189\224\78\223\43\183\139"));
	if playerGui then
		local gameUI = playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\253\135\19\244\7", "\161\78\156\234\118"));
		local topFrame = gameUI and gameUI:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\147\184\217\250\181\182\196\217", "\188\199\215\169"));
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\219\57\107\114\252\240\12", "\136\156\105\63\27"));
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\47\137\97\32\55\141\123\49\23", "\84\123\236\25"));
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\217\165\158\50\158\152\217\184\153\62\131\155", "\213\144\235\202\119\204"))) then
			inIntermission = true;
		end
	end
	previousInMatch = isInMatch;
	isInMatch = mapExists and not inIntermission;
	if (previousInMatch and not isInMatch) then
		cleanupAllESP();
	end
end
updateMatchState();
