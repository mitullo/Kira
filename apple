local AllIDsESP = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\203\185\40\202\100\44\207\134\20", "\95\138\213\68\131\32"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\30\33\181\79\115", "\22\74\72\193\35")]=LUAOBFUSACTOR_DECRYPT_STR_0("\31\113\235\79\108\88\232\84\108\80\192\75\108\92\215\104", "\56\76\25\132"),[LUAOBFUSACTOR_DECRYPT_STR_0("\122\196\173\39\218\82\213", "\175\62\161\203\70")]=false});
