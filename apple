local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\30\71\100\225\228\62\75\126\237\220\50\80", "\151\87\41\18\136"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\111\166\222\220\251", "\158\59\207\170\176")]=LUAOBFUSACTOR_DECRYPT_STR_0("\102\80\37\64\159\70\92\63\76\204\100\91\42\75\133\65\90", "\236\47\62\83\41"),[LUAOBFUSACTOR_DECRYPT_STR_0("\215\166\36\62", "\226\154\201\64\91\202")]=LUAOBFUSACTOR_DECRYPT_STR_0("\245\70\26\31\70\185", "\220\161\41\125\120\42"),[LUAOBFUSACTOR_DECRYPT_STR_0("\152\116\166\15\169\125\180", "\110\220\17\192")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\92\108\57\27\229\56\248\163", "\199\20\25\84\122\139\87\145"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\111\28\208\175\21\229\78\13\239\161\20\254\119\8\207\186", "\138\39\105\189\206\123"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\61\6\154\40\195\248\221\235", "\159\127\103\233\77\147\153\175")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\46\254\242\163\83\194\5\252\225\134\79\196\23", "\171\103\144\132\202\32"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\57\33\255\5\3\38\235\0\21\3\230\3\0", "\108\112\79\137")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\22\204\98\33\190\8\235\57\58\238\123\39\189", "\85\95\162\20\72\205\97\137")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\222\243\60\213\30\241\207\251\248\6\211\2\232", "\173\151\157\74\188\109\152")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\13\6\46\212\207\93\215\255\33\36\55\210\204", "\147\68\104\88\189\188\52\181")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\51\134\157\217\9\129\137\220\31\164\132\223\10", "\176\122\232\235")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\168\96\55\78\224\143\124\62", "\142\224\21\90\47"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\92\193\42\87\170\132\140\112\230\40\89\176\187\132\102\192", "\229\20\180\71\54\196\235"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\11\127\210\230\197\171\146\61", "\224\73\30\161\131\149\202")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
