local isInMatch = false;
local previousInMatch = false;
local function updateMatchState()
	local mapExists = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\203\254\154", "\177\134\159\234\195")) ~= nil;
	local inIntermission = false;
	local playerGui = LocalPlayer:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\141\231\62\185\204\175\204\42\169", "\169\221\139\95\192"));
	if playerGui then
		local gameUI = playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\249\138\114\58\23\15", "\70\190\235\31\95\66"));
		local topFrame = gameUI and gameUI:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\142\237\10\192\247\187\239\31", "\133\218\130\122\134"));
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\27\207\215\205\200\175\61", "\88\92\159\131\164\188\195"));
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\180\43\167\95\251\234\223\133\34", "\189\224\78\223\43\183\139"));
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\7\210\190\51\243\3\213\185\37\232\1\210", "\161\78\156\234\118"))) then
			inIntermission = true;
		end
	end
	previousInMatch = isInMatch;
	isInMatch = mapExists and not inIntermission;
	if (previousInMatch and not isInMatch) then
		cleanupAllESP();
	end
end
