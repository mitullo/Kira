local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\35\223\217\44", "\22\107\186\184\72\36\204"),LUAOBFUSACTOR_DECRYPT_STR_0("\203\184\34\90\40\232\178\48", "\110\135\221\68\46"),LUAOBFUSACTOR_DECRYPT_STR_0("\207\51\10\255\230\178\53\231", "\91\131\86\108\139\174\211"),LUAOBFUSACTOR_DECRYPT_STR_0("\215\46\190\3\113\244\60\189\5\124\233\38", "\61\155\75\216\119"),LUAOBFUSACTOR_DECRYPT_STR_0("\40\174\180\40\116\6\202\1\185\158\57\95", "\189\100\203\210\92\56\105"),LUAOBFUSACTOR_DECRYPT_STR_0("\3\84\251\60\26\65\237\45\61\112\239\37", "\72\79\49\157"),LUAOBFUSACTOR_DECRYPT_STR_0("\164\181\55\168\189\160\33\185\154\156\52\187", "\220\232\208\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\183\226\56\56\124\174\250\170", "\193\149\222\133\80\76\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\244\84\72\218\210\117\78\220\194", "\178\166\61\47"),LUAOBFUSACTOR_DECRYPT_STR_0("\201\67\239\114\222\18\244\93\237\104\235\44\246", "\94\155\42\136\26\170"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\54\33\189\144\19\41\162\129\45\10\176\131", "\213\228\95\70"),LUAOBFUSACTOR_DECRYPT_STR_0("\24\178\197\140\99\31\171\210\129\101\11\169\207", "\23\74\219\162\228"),LUAOBFUSACTOR_DECRYPT_STR_0("\11\239\65\167\47\12\246\86\170\41\21\227\65", "\91\89\134\38\207")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\113\254\216\51\1\228\40\86\253\199", "\71\36\142\168\86\115\176"),LUAOBFUSACTOR_DECRYPT_STR_0("\243\174\101\186\17\138\89\91\204\174", "\41\191\193\18\223\99\222\54"),LUAOBFUSACTOR_DECRYPT_STR_0("\131\51\202\43\164\164\47\195\24\165\164\50\247\43\184\191", "\202\203\70\167\74")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\14\0\207\54\65\45\19\200", "\17\76\97\188\83"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\167\38\202\50\0\130\89\183", "\195\229\71\185\87\80\227\43"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\213\236\16\85\253\212\243\18\67\224", "\143\128\156\96\48"),LUAOBFUSACTOR_DECRYPT_STR_0("\148\222\231\23\5\140\222\226\1\24", "\119\216\177\144\114"),LUAOBFUSACTOR_DECRYPT_STR_0("\225\60\244\67\199\38\240\70\251\38\246\86\249\40\235\86", "\34\169\73\153")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\136\237\24\142\154\237\25\159", "\235\202\140\107"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
local InvisibleMode = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\37\122\34\161\250\46\245\201\9\89\59\172\236", "\165\108\20\84\200\137\71\151"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\78\189\63\132\127", "\232\26\212\75")]=LUAOBFUSACTOR_DECRYPT_STR_0("\30\71\100\225\228\62\75\126\237\183\26\70\118\237", "\151\87\41\18\136"),[LUAOBFUSACTOR_DECRYPT_STR_0("\127\170\204\209\235\87\187", "\158\59\207\170\176")]=false});
local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\102\80\37\64\159\70\92\63\76\167\74\71", "\236\47\62\83\41"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\206\160\52\55\175", "\226\154\201\64\91\202")]=LUAOBFUSACTOR_DECRYPT_STR_0("\232\71\11\17\89\181\195\69\24\88\97\185\216\75\20\22\78", "\220\161\41\125\120\42"),[LUAOBFUSACTOR_DECRYPT_STR_0("\145\126\164\11", "\110\220\17\192")]=LUAOBFUSACTOR_DECRYPT_STR_0("\64\118\51\29\231\50", "\199\20\25\84\122\139\87\145"),[LUAOBFUSACTOR_DECRYPT_STR_0("\99\12\219\175\14\230\83", "\138\39\105\189\206\123")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\55\18\132\44\253\246\198\251", "\159\127\103\233\77\147\153\175"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\47\229\233\171\78\196\14\244\214\165\79\223\55\241\246\190", "\171\103\144\132\202\32"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\50\46\250\9\32\46\251\24", "\108\112\79\137")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\22\204\98\33\190\8\235\57\58\238\123\39\189", "\85\95\162\20\72\205\97\137"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\222\243\60\213\30\241\207\251\248\6\211\2\232", "\173\151\157\74\188\109\152")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\13\6\46\212\207\93\215\255\33\36\55\210\204", "\147\68\104\88\189\188\52\181")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\51\134\157\217\9\129\137\220\31\164\132\223\10", "\176\122\232\235")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\169\123\44\70\253\137\119\54\74\194\143\122\42", "\142\224\21\90\47")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\93\218\49\95\183\130\135\120\209\11\89\171\155", "\229\20\180\71\54\196\235")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\1\107\204\226\251\165\137\45", "\224\73\30\161\131\149\202"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\217\240\252\81\255\234\248\84\195\234\254\68\193\228\227\68", "\48\145\133\145"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\120\77\166\235\225\45\72\88", "\76\58\44\213\142\177")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
