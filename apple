local QuickCollectAllIDs = Tabs.IDSystem:AddButton({[LUAOBFUSACTOR_DECRYPT_STR_0("\44\74\247\198\3", "\43\120\35\131\170\102\54")]=LUAOBFUSACTOR_DECRYPT_STR_0("\101\19\142\181\174\240\167\91\10\139\179\166\164\196\117\10\139\246\140\148\151", "\228\52\102\231\214\197\208"),[LUAOBFUSACTOR_DECRYPT_STR_0("\58\229\102\201\248\130\9\194\23\239\123", "\182\126\128\21\170\138\235\121")]=LUAOBFUSACTOR_DECRYPT_STR_0("\170\207\33\233\139\18\36\15\136\219\57\234\159\83\51\9\135\214\48\229\146\83\49\10\135\154\37\234\135\10\53\20\203\243\17\245", "\102\235\186\85\134\230\115\80"),[LUAOBFUSACTOR_DECRYPT_STR_0("\116\13\50\83\112\213\33\92", "\66\55\108\94\63\18\180")]=function()
