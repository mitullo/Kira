local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\222\38\0", "\123\147\71\112\127\122"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\229\201", "\38\172\173\226\17")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\100\21\28\253\66\28\60\251", "\143\45\113\76"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\136\180\29\37\189\170\50\61\181\189", "\92\216\216\124"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\27\122", "\157\59\82\204\32") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\6\118\216\196\161\215\152\248", "\209\88\94\131\154\137\138\179")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\109\178\143\56", "\66\72\193\164\28\126\67\81"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\207\57\165\89\40\121\238\40\154\87\41\98\215\45\186\76", "\22\135\76\200\56\70"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\185\57\236\40\88", "\129\237\80\152\68\61")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\114\167\10\231\25\25\76", "\56\49\200\100\147\124\119")]=LUAOBFUSACTOR_DECRYPT_STR_0("\239\54\190\226\205\61\171\245\222\126\177\255\216\126\185\255\217\48\187", "\144\172\94\223"),[LUAOBFUSACTOR_DECRYPT_STR_0("\0\26\176\70\48\6\173\73", "\39\68\111\194")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\251\167\247", "\215\182\198\135\167\25"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\164\77", "\40\237\41\138")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\244\97\232\254\75\196\113\221\237\67", "\42\167\20\154\152"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\99\250\146\80\126\44\90\234", "\65\42\158\194\34\17"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\42\43\83\21\40\255\53\239\23\34", "\142\122\71\50\108\77\141\123"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\33\171\235\20\62", "\91\117\194\159\120")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\57\18\48\12\48\255\48", "\68\122\125\94\120\85\145")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\36\9\204\93\205\202\169\17\9\195\82\209\153\185\24\16\195\91\203\205\191\19\92", "\218\119\124\175\62\168\185") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\129\229\90\197\177\249\71\202", "\164\197\144\40")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\183\249\190\135\216", "\214\227\144\202\235\189")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\206\170\137\111\21\189\71", "\92\141\197\231\27\112\211\51")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\197\240\159\175\213\166\241\133\183\145\224\246\132\167\145", "\177\134\159\234\195") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\153\254\45\161\221\180\228\49", "\169\221\139\95\192")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
