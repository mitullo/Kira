local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\23\46\123\242", "\224\95\75\26\150\169\181\180"),LUAOBFUSACTOR_DECRYPT_STR_0("\39\223\222\60\98\163\121\31", "\22\107\186\184\72\36\204"),LUAOBFUSACTOR_DECRYPT_STR_0("\203\184\34\90\38\230\179\32", "\110\135\221\68\46"),LUAOBFUSACTOR_DECRYPT_STR_0("\207\51\10\255\226\188\44\230\36\45\249\195", "\91\131\86\108\139\174\211"),LUAOBFUSACTOR_DECRYPT_STR_0("\215\46\190\3\113\244\60\189\5\113\254\44", "\61\155\75\216\119"),LUAOBFUSACTOR_DECRYPT_STR_0("\40\174\180\40\109\25\205\1\185\147\46\85", "\189\100\203\210\92\56\105"),LUAOBFUSACTOR_DECRYPT_STR_0("\3\84\251\60\26\65\237\45\61\125\248\47", "\72\79\49\157"),LUAOBFUSACTOR_DECRYPT_STR_0("\186\185\54\180\156\150\62\179\156", "\220\232\208\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\183\226\56\56\114\160\251\186", "\193\149\222\133\80\76\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\244\84\72\218\210\113\64\197\195\79\110\192\203", "\178\166\61\47"),LUAOBFUSACTOR_DECRYPT_STR_0("\201\67\239\114\222\18\244\93\237\104\230\59\252", "\94\155\42\136\26\170"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\54\33\189\144\10\54\165\129\45\7\167\137", "\213\228\95\70"),LUAOBFUSACTOR_DECRYPT_STR_0("\24\178\197\140\99\31\171\210\129\101\6\190\197", "\23\74\219\162\228")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\12\246\86\170\41\13\233\84\188\52", "\91\89\134\38\207"),LUAOBFUSACTOR_DECRYPT_STR_0("\104\225\223\51\1\228\40\86\253\199", "\71\36\142\168\86\115\176"),LUAOBFUSACTOR_DECRYPT_STR_0("\247\180\127\190\13\177\95\77\237\174\125\171\51\191\68\93", "\41\191\193\18\223\99\222\54")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\137\39\212\47\154\170\52\211", "\202\203\70\167\74"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\14\0\207\54\65\45\19\200", "\17\76\97\188\83"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\176\55\201\50\34\183\68\177\150\40", "\195\229\71\185\87\80\227\43"),LUAOBFUSACTOR_DECRYPT_STR_0("\204\243\23\85\253\212\243\18\67\224", "\143\128\156\96\48"),LUAOBFUSACTOR_DECRYPT_STR_0("\144\196\253\19\25\183\216\244\32\24\183\197\192\19\5\172", "\119\216\177\144\114")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\235\40\234\71\249\40\235\86", "\34\169\73\153"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
local InvisibleMode = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\131\226\29\130\185\229\9\135\175\193\4\143\175", "\235\202\140\107"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\56\125\32\164\236", "\165\108\20\84\200\137\71\151")]=LUAOBFUSACTOR_DECRYPT_STR_0("\83\186\61\129\105\189\41\132\127\244\6\135\126\177", "\232\26\212\75"),[LUAOBFUSACTOR_DECRYPT_STR_0("\19\76\116\233\226\59\93", "\151\87\41\18\136")]=false});
local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\114\161\220\217\237\82\173\198\213\213\94\182", "\158\59\207\170\176"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\123\87\39\69\137", "\236\47\62\83\41")]=LUAOBFUSACTOR_DECRYPT_STR_0("\211\167\54\50\185\139\248\165\37\123\129\135\227\171\41\53\174", "\226\154\201\64\91\202"),[LUAOBFUSACTOR_DECRYPT_STR_0("\236\70\25\29", "\220\161\41\125\120\42")]=LUAOBFUSACTOR_DECRYPT_STR_0("\136\126\167\9\176\116", "\110\220\17\192"),[LUAOBFUSACTOR_DECRYPT_STR_0("\80\124\50\27\254\59\229", "\199\20\25\84\122\139\87\145")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\111\28\208\175\21\229\78\13", "\138\39\105\189\206\123"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\55\18\132\44\253\246\198\251\45\8\134\57\195\248\221\235", "\159\127\103\233\77\147\153\175"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\37\241\247\175\112\202\21\228", "\171\103\144\132\202\32")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\57\33\255\5\3\38\235\0\21\3\230\3\0", "\108\112\79\137"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\22\204\98\33\190\8\235\57\58\238\123\39\189", "\85\95\162\20\72\205\97\137")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\222\243\60\213\30\241\207\251\248\6\211\2\232", "\173\151\157\74\188\109\152")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\13\6\46\212\207\93\215\255\33\36\55\210\204", "\147\68\104\88\189\188\52\181")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\51\134\157\217\9\129\137\220\31\164\132\223\10", "\176\122\232\235")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\169\123\44\70\253\137\119\54\74\194\143\122\42", "\142\224\21\90\47")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\92\193\42\87\170\132\140\112", "\229\20\180\71\54\196\235"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\1\107\204\226\251\165\137\45\76\206\236\225\154\129\59\106", "\224\73\30\161\131\149\202"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\211\228\226\85\193\228\227\68", "\48\145\133\145")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
	end
end);
