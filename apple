local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\248\96\192\54\94\55\17\224\125\215\54\97\55\22\217\117\194\50", "\121\171\20\165\87\50\67")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\224\55\181\50\188\16", "\98\166\88\217\86\217"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\197\226\124\0\138\200\254\221\112\19\135\239\226\249\107\0\129\217", "\188\150\150\25\97\230");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\233\157\90\3\0\249\210\173\90\3\24\229\249\129\90\1\7", "\141\186\233\63\98\108"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\213\239\45\162\45\223\229\56\179\2\228\227", "\69\145\138\76\214"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\67\219\140\136\179\2\120\232\156\128\155\19\100\202\138\157", "\118\16\175\233\233\223"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\175\129\52\175\230\165\114\159\129", "\29\235\228\85\219\142\235")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\25\209\187\201\127\96\40\70\56\243\175\212", "\50\93\180\218\189\23\46\71");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\238\182\94\90\65\210\92\250\161\72\88\86\211\81", "\40\190\196\59\44\36\188"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\8\76\200\184\255", "\109\92\37\188\212\154\29")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\39\224\170\215\52\84\16", "\58\100\143\196\163\81")]=LUAOBFUSACTOR_DECRYPT_STR_0("\41\86\38\162\51\93\237\78\49\75\49\162\127\91\224\15\30\91\98", "\110\122\34\67\195\95\41\133"),[LUAOBFUSACTOR_DECRYPT_STR_0("\81\164\73\75\194\124\190\85", "\182\21\209\59\42")]=3});
		end
	end));
end
setupDeathNoteStorage();
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.StealthKiraKey.Value])) then
		Options.StealthKira:SetValue(not Options.StealthKira.Value);
	end
end);
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\147\82\196\9\41\144\184\67\192\58\52\183", "\222\215\55\165\125\65"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\8\212\199\14\250\239\226\94\41\246\211\19", "\42\76\177\166\122\146\161\141"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\131\152\4\195\124", "\22\197\234\101\174\25")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\11\38\164\209\115", "\230\77\84\197\188\22\207\183"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\206\6\207\232\137", "\85\153\116\166\156\236\193\144"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\138\225\64\182\247", "\96\196\128\45\211\132"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\24\132\104\94", "\184\85\237\27\63\178\207\212")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\60\80\29\83\13", "\63\104\57\105")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\40\136\170\80\14\137\176", "\36\107\231\196")]=LUAOBFUSACTOR_DECRYPT_STR_0("\104\166\167\199\121\176\163\147\85\245\140\136\73\176\226\136\83\182\167\199\91\188\176\148\73\251", "\231\61\213\194"),[LUAOBFUSACTOR_DECRYPT_STR_0("\45\184\47\114\29\164\50\125", "\19\105\205\93")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\136\29\202\142\127\141\13\223\149\55\233\38\209\149\58\233\46\215\143\59\172\26", "\95\201\104\190\225"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\137\194\207\202\139\206\192\218\167\229\206\218\170", "\174\207\171\161"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\217\247\25\255\253", "\183\141\158\109\147\152")]=LUAOBFUSACTOR_DECRYPT_STR_0("\13\28\242\3\108\47\239\2\40\73\194\9\45\29\238\76\2\6\242\9", "\108\76\105\134"),[LUAOBFUSACTOR_DECRYPT_STR_0("\207\192\183\224\219\231\209", "\174\139\165\209\129")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\142\178\242", "\24\195\211\130\161\166\99\16"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\101\17\232\56\86", "\118\38\99\137\76\51")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\223\47\11\34\27\47\240\54\17", "\64\157\70\101\114\105"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\104\189\170\226\30\79\161\163\209\31\79\188\151\226\2\84", "\112\32\200\199\131"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\14\89\82\136\209\164\47\60\68", "\66\76\48\60\216\163\203"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\147\136\106\231\94\192\48\142\135\114\246\118\234\55", "\68\218\230\25\147\63\174"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\153\35\71\64\179", "\214\205\74\51\44")]=LUAOBFUSACTOR_DECRYPT_STR_0("\211\66\241\232\118\244\88\162\200\118\241\73\162\213\83\233", "\23\154\44\130\156"),[LUAOBFUSACTOR_DECRYPT_STR_0("\53\163\171\175\35\31\5", "\115\113\198\205\206\86")]=false});
