	if (((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\239\113\196\35\90\13\22\223\113", "\121\171\20\165\87\50\67")) or (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\226\61\184\34\177\44\201\44\188\17\172\11", "\98\166\88\217\86\217"))) and Options.InstantKill.Value) then
