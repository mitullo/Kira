	local deathNoteGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\16\181\34\242\60\158\44\242\49", "\134\84\208\67")) or LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\55\169\135\72\27\130\137\72\22\139\147\85", "\60\115\204\230"));
	if deathNoteGui then
		setupInstantKill(deathNoteGui, enabled);
	end
end);
