local AllIDsESP = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\13\117\232\113\8\106\193\107\28", "\56\76\25\132"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\106\200\191\42\202", "\175\62\161\203\70")]=LUAOBFUSACTOR_DECRYPT_STR_0("\15\213\204\4\117\29\209\207\83\28\24\206\131\54\6\12", "\85\92\189\163\115"),[LUAOBFUSACTOR_DECRYPT_STR_0("\13\169\54\57\60\160\36", "\88\73\204\80")]=false});
