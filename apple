local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\31\141\29\116\25", "\33\80\126\224\120"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\197\134\55\225\110\193\129\48\247\117\195\134", "\60\140\200\99\164"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\160\245\9\35\151\174", "\194\231\148\100\70"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\114\73\217\183", "\168\38\44\161\195\150")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\169\210\182\83\2\197\159\37\179\213\173\88", "\118\224\156\226\22\80\136\214")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\101\239\84\133\119\199", "\224\34\142\57")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\158\239", "\110\190\199\165\189\19\145\61") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\238\226\99\228\142", "\167\186\139\23\136\235")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\57\186\134\25\31\187\156", "\109\122\213\232")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\174\255\163\35\174\246\226\20\235\246\182\56\174\217\173\36\235\182", "\80\142\151\194")),[LUAOBFUSACTOR_DECRYPT_STR_0("\39\211\101\77\23\207\120\66", "\44\99\166\23")]=8});
end
