local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\98\170\36\10\49\246\231\66\160\50", "\147\54\207\92\126\115\131")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\37\30\25\89\77\86\40\3\16\61\57\81\77\20\25\84\32\87\35\16\1\88", "\30\109\81\85\29\109"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\211\126\87\183\58\237\255\237\120\68\162", "\156\159\17\52\214\86\190"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\135\225\174\168\175\225\169\151\167\227\177\157\173\251\180\170\175\251\184\184\145", "\220\206\143\221") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\136\116\33", "\178\230\29\77\119\184\172"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\198\170\11\15\98\235", "\152\149\222\106\123\23")]=LUAOBFUSACTOR_DECRYPT_STR_0("\248\62\243\64\160\201\35\208\81\186\208\2\243\66\161\213\8\249\87\176", "\213\189\70\150\35"),[LUAOBFUSACTOR_DECRYPT_STR_0("\123\84\102\15\74\65", "\104\47\53\20")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\128\77\148\15\185", "\111\195\44\225\124\220")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\241\72\19\103\170\165\204\109\9\127\167\142\206\67\14\103\148", "\203\184\38\96\19\203") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\28\107\124\66\219\45\118\95\83\193\52\87\124\64\218\49\93\118\85\203", "\174\89\19\25\33")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\28\39\113\109\210\180\56", "\107\79\114\50\46\151\231");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\17\137\153\13\202\17\146\242\28\230\129\6\202\28\155\233\20\143\155\8\190\28", "\160\89\198\213\73\234\89\215");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\124\67\141\190\228\111\80\157\208", "\165\40\17\212\158");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\205\246\36\23\102\205\252\58\22\102\209\246\72\22\10\204\244\33\29\7\209\252", "\70\133\185\104\83");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\45\75\87\62\200\10\81\111\35\197\8\100\71\62\192\18\68\80\47\205\59", "\169\100\37\36\74") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\41\137\177\68\1\137\182\123\9\139\174\113\3\147\171\70\1\147\167\84\63", "\48\96\231\194") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\225\84\29\57\24\214\187\168\193\86\2\12\26\204\166\149\201\78\11\41\38", "\227\168\58\110\77\121\184\207") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\82\50\172\84\176\213\101\142\114\48\179\101\167\222\127\177\68", "\197\27\92\223\32\209\187\17") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\81\208\239\2\81\215\208\10\83\207\222\21\90\205\239\60", "\155\99\63\163") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\171\223\178\153\184\138\150\250\168\129\181\161\148\212\175\153\134", "\228\226\177\193\237\217") .. gui.Name] = nil;
		end
	end
end
