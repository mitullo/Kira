	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\131\24\1\191\112\129", "\80\196\121\108\218\37\200\213"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\41\93\54\90\121\35\163\51\64\43\80\101", "\234\96\19\98\31\43\110"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\33\30\95\194\153\91", "\235\102\127\50\167\204\18"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\100\164\237\55", "\78\48\193\149\67\36")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\25\48\180\61\115\29\55\179\43\104\31\48", "\33\80\126\224\120")) then
					clearKiraList();
				end
			end);
		end
	end
end
