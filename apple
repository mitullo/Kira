local isInMatch = false;
local previousInMatch = false;
local function updateMatchState()
	local mapExists = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\144\234\47", "\169\221\139\95\192")) ~= nil;
	local inIntermission = false;
	local playerGui = LocalPlayer:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\135\126\38\39\52\249\158\118", "\70\190\235\31\95\66"));
	if playerGui then
		local gameUI = playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\157\227\23\227\208\147", "\133\218\130\122\134"));
		local topFrame = gameUI and gameUI:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\8\240\243\226\206\162\53\57", "\88\92\159\131\164\188\195"));
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\167\30\139\66\195\231\216", "\189\224\78\223\43\183\139"));
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\26\249\146\2\237\47\254\143\26", "\161\78\156\234\118"));
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\142\153\253\249\149\154\224\239\148\158\230\242", "\188\199\215\169"))) then
			inIntermission = true;
		end
	end
	previousInMatch = isInMatch;
	isInMatch = mapExists and not inIntermission;
	if (previousInMatch and not isInMatch) then
		cleanupAllESP();
	end
end
updateMatchState();
