local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\81\219\200", "\178\28\186\184\61\55\83"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\237\201", "\149\164\173\39\92\146\110")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\218\35\32\13\21\22\227\51", "\123\147\71\112\127\122"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\252\193\131\104\67\222\227\131\124\67", "\38\172\173\226\17"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\13\89", "\143\45\113\76") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\134\240\39\2\240\133\87\117", "\92\216\216\124")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\30\33\231\4", "\157\59\82\204\32"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\16\43\238\251\231\229\218\181\10\49\236\238\217\235\193\165", "\209\88\94\131\154\137\138\179"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\28\168\208\112\27", "\66\72\193\164\28\126\67\81")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\196\35\166\76\35\120\243", "\22\135\76\200\56\70")]=LUAOBFUSACTOR_DECRYPT_STR_0("\174\56\249\54\92\226\153\53\234\100\83\238\153\112\254\43\72\239\137", "\129\237\80\152\68\61"),[LUAOBFUSACTOR_DECRYPT_STR_0("\117\189\22\242\8\30\87\95", "\56\49\200\100\147\124\119")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\225\63\175", "\144\172\94\223"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\13\11", "\39\68\111\194")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\229\179\245\193\120\180\211\129\242\206", "\215\182\198\135\167\25"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\164\77\218\90\130\68\250\92", "\40\237\41\138"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\247\120\251\225\79\213\90\251\245\79", "\42\167\20\154\152"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\126\247\182\78\116", "\65\42\158\194\34\17")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\57\40\92\24\40\227\15", "\142\122\71\50\108\77\141\123")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\38\183\252\27\62\6\177\249\13\55\25\187\191\27\52\25\174\250\27\47\16\166\191", "\91\117\194\159\120") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\62\8\44\25\33\248\43\20", "\68\122\125\94\120\85\145")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\35\21\219\82\205", "\218\119\124\175\62\168\185")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\134\255\70\208\160\254\92", "\164\197\144\40")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\160\255\191\135\217\246\141\255\190\203\219\191\141\244\234", "\214\227\144\202\235\189") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\201\176\149\122\4\186\92\50", "\92\141\197\231\27\112\211\51")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
