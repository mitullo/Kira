local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\96\16\60", "\143\45\113\76"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\145\188", "\92\216\216\124")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\114\54\156\82\242\86\34\184", "\157\59\82\204\32"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\8\50\226\227\236\248\253\176\53\59", "\209\88\94\131\154\137\138\179"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\104\233", "\66\72\193\164\28\126\67\81") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\217\100\147\102\110\75\172\101", "\22\135\76\200\56\70")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\200\35\179\96", "\129\237\80\152\68\61"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\121\189\9\242\18\24\81\85\154\11\252\8\39\89\67\188", "\56\49\200\100\147\124\119"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\248\55\171\252\201", "\144\172\94\223")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\7\0\172\83\33\1\182", "\39\68\111\194")]=LUAOBFUSACTOR_DECRYPT_STR_0("\245\174\230\213\120\180\194\163\245\135\119\184\194\230\225\200\108\185\210", "\215\182\198\135\167\25"),[LUAOBFUSACTOR_DECRYPT_STR_0("\169\92\248\73\153\64\229\70", "\40\237\41\138")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\234\117\234", "\42\167\20\154\152"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\99\250", "\65\42\158\194\34\17")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\41\50\64\10\44\238\30\201\15\46", "\142\122\71\50\108\77\141\123"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\60\166\207\10\52\24\178\235", "\91\117\194\159\120"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\42\17\63\1\48\227\10\27\16\59", "\68\122\125\94\120\85\145"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\35\21\219\82\205", "\218\119\124\175\62\168\185")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\134\255\70\208\160\254\92", "\164\197\144\40")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\176\229\169\136\216\165\144\246\191\135\209\175\195\243\165\135\209\179\128\228\175\143\157", "\214\227\144\202\235\189") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\201\176\149\122\4\186\92\50", "\92\141\197\231\27\112\211\51")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\210\246\158\175\212", "\177\134\159\234\195")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\158\228\49\180\204\179\255", "\169\221\139\95\192")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\253\132\106\51\38\102\208\132\107\127\36\47\208\143\63", "\70\190\235\31\95\66") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\158\247\8\231\241\179\237\20", "\133\218\130\122\134")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
