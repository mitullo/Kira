local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\99\93\28\202\117\77\16\202\88\86", "\190\55\56\100")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\126\128\16\58\83\203\214\100\138\124\42\60\163\214\122\134\17\55\61\194\199\115", "\147\54\207\92\126\115\131"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\33\62\54\124\1\77\14\35\60\109\25", "\30\109\81\85\29\109"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\214\127\71\162\55\208\232\212\120\88\186\23\221\232\246\103\85\162\51\218\195", "\156\159\17\52\214\86\190") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\160\230\177", "\220\206\143\221"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\181\105\44\3\205\223", "\178\230\29\77\119\184\172")]=LUAOBFUSACTOR_DECRYPT_STR_0("\208\166\15\24\98\236\240\152\24\20\122\220\240\191\30\19\89\247\225\187", "\152\149\222\106\123\23"),[LUAOBFUSACTOR_DECRYPT_STR_0("\233\39\228\68\176\201", "\213\189\70\150\35")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\108\84\97\27\74", "\104\47\53\20")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\138\66\146\8\189\1\183\103\136\16\176\42\181\73\143\8\131", "\111\195\44\225\124\220") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\253\94\5\112\190\191\221\96\18\124\166\143\221\71\20\123\133\164\204\67", "\203\184\38\96\19\203")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\10\70\90\98\235\10\64", "\174\89\19\25\33");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\7\61\126\106\183\175\46\29\55\18\122\216\199\46\3\59\127\103\217\166\63\10", "\107\79\114\50\46\151\231");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\13\148\140\105\171\30\150\233\23", "\160\89\198\213\73\234\89\215");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\96\94\152\218\133\96\84\134\219\133\124\94\244\219\233\97\92\157\208\228\124\84", "\165\40\17\212\158");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\204\215\27\39\39\235\205\35\58\42\233\248\11\39\47\243\216\28\54\34\218", "\70\133\185\104\83") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\45\75\87\62\200\10\81\111\35\197\8\100\71\62\192\18\68\80\47\205\59", "\169\100\37\36\74") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\41\137\177\68\1\137\182\123\9\139\174\113\3\147\171\70\1\147\167\84\63", "\48\96\231\194") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\225\84\29\57\24\214\187\168\193\86\2\8\15\221\161\151\247", "\227\168\58\110\77\121\184\207") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\82\50\172\84\176\213\101\142\114\48\179\101\167\222\127\177\68", "\197\27\92\223\32\209\187\17") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\81\208\239\2\81\215\208\10\83\207\222\21\90\205\239\60", "\155\99\63\163") .. gui.Name] = nil;
		end
	end
end
