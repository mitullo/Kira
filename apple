	local deathNoteGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\55\169\135\72\27\130\137\72\22", "\60\115\204\230")) or LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\195\63\234\100\239\20\228\100\226\29\254\121", "\16\135\90\139"));
	if deathNoteGui then
		setupInstantKill(deathNoteGui, enabled);
	end
end);
