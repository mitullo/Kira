local AllIDsESP = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\127\205\167\15\235\77\228\152\22", "\175\62\161\203\70"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\8\212\215\31\48", "\85\92\189\163\115")]=LUAOBFUSACTOR_DECRYPT_STR_0("\26\164\63\47\105\141\60\52\105\133\20\43\105\137\3\8", "\88\73\204\80"),[LUAOBFUSACTOR_DECRYPT_STR_0("\10\134\22\71\60\214\58", "\186\78\227\112\38\73")]=false});
