local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\207\184\37\74", "\110\135\221\68\46"),LUAOBFUSACTOR_DECRYPT_STR_0("\207\51\10\255\232\188\52\247", "\91\131\86\108\139\174\211"),LUAOBFUSACTOR_DECRYPT_STR_0("\215\46\190\3\117\250\37\188", "\61\155\75\216\119"),LUAOBFUSACTOR_DECRYPT_STR_0("\40\174\180\40\116\6\202\1\185\147\46\85", "\189\100\203\210\92\56\105"),LUAOBFUSACTOR_DECRYPT_STR_0("\3\84\251\60\3\94\234\45\61\125\248\47", "\72\79\49\157"),LUAOBFUSACTOR_DECRYPT_STR_0("\164\181\55\168\189\160\33\185\154\145\35\177", "\220\232\208\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\217\187\227\36\25\74\177\240\172\201\53\43", "\193\149\222\133\80\76\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\244\84\72\218\210\123\64\221\210", "\178\166\61\47"),LUAOBFUSACTOR_DECRYPT_STR_0("\201\67\239\114\222\22\250\68\236", "\94\155\42\136\26\170"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\54\33\189\144\19\41\162\129\45\7\167\137", "\213\228\95\70"),LUAOBFUSACTOR_DECRYPT_STR_0("\24\178\197\140\99\6\180\213\129\101\6\190\197", "\23\74\219\162\228"),LUAOBFUSACTOR_DECRYPT_STR_0("\11\239\65\167\47\12\246\86\170\41\24\244\75", "\91\89\134\38\207"),LUAOBFUSACTOR_DECRYPT_STR_0("\118\231\207\62\7\229\55\84\235\218\26\22\215", "\71\36\142\168\86\115\176")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\234\177\98\186\17\138\89\91\204\174", "\41\191\193\18\223\99\222\54"),LUAOBFUSACTOR_DECRYPT_STR_0("\135\41\208\47\184\159\41\213\57\165", "\202\203\70\167\74"),LUAOBFUSACTOR_DECRYPT_STR_0("\4\20\209\50\127\35\8\216\1\126\35\21\236\50\99\56", "\17\76\97\188\83")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\167\38\202\50\0\130\89\183", "\195\229\71\185\87\80\227\43"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\194\253\19\85\223\225\238\20", "\143\128\156\96\48"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\141\193\224\23\5\140\222\226\1\24", "\119\216\177\144\114"),LUAOBFUSACTOR_DECRYPT_STR_0("\229\38\238\71\219\29\246\80\218\38", "\34\169\73\153"),LUAOBFUSACTOR_DECRYPT_STR_0("\130\249\6\138\164\227\2\143\152\227\4\159\154\237\25\159", "\235\202\140\107")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\46\117\39\173\217\38\229\209", "\165\108\20\84\200\137\71\151"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
local InvisibleMode = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\83\186\61\129\105\189\41\132\127\153\36\140\127", "\232\26\212\75"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\3\64\102\228\242", "\151\87\41\18\136")]=LUAOBFUSACTOR_DECRYPT_STR_0("\114\161\220\217\237\82\173\198\213\190\118\160\206\213", "\158\59\207\170\176"),[LUAOBFUSACTOR_DECRYPT_STR_0("\107\91\53\72\153\67\74", "\236\47\62\83\41")]=false});
local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\211\167\54\50\185\139\248\165\37\16\175\155", "\226\154\201\64\91\202"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\245\64\9\20\79", "\220\161\41\125\120\42")]=LUAOBFUSACTOR_DECRYPT_STR_0("\149\127\182\7\175\120\162\2\185\49\139\11\165\115\169\0\184", "\110\220\17\192"),[LUAOBFUSACTOR_DECRYPT_STR_0("\89\118\48\31", "\199\20\25\84\122\139\87\145")]=LUAOBFUSACTOR_DECRYPT_STR_0("\115\6\218\169\23\239", "\138\39\105\189\206\123"),[LUAOBFUSACTOR_DECRYPT_STR_0("\59\2\143\44\230\245\219", "\159\127\103\233\77\147\153\175")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\47\229\233\171\78\196\14\244", "\171\103\144\132\202\32"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\56\58\228\13\30\32\224\8\34\32\230\24\32\46\251\24", "\108\112\79\137"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\29\195\103\45\157\0\251\33", "\85\95\162\20\72\205\97\137")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\222\243\60\213\30\241\207\251\248\6\211\2\232", "\173\151\157\74\188\109\152"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\13\6\46\212\207\93\215\255\33\36\55\210\204", "\147\68\104\88\189\188\52\181")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\51\134\157\217\9\129\137\220\31\164\132\223\10", "\176\122\232\235")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\169\123\44\70\253\137\119\54\74\194\143\122\42", "\142\224\21\90\47")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\93\218\49\95\183\130\135\120\209\11\89\171\155", "\229\20\180\71\54\196\235")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\0\112\215\234\230\163\130\37\123\237\236\250\186", "\224\73\30\161\131\149\202")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\217\240\252\81\255\234\248\84", "\48\145\133\145"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\114\89\184\239\223\35\83\72\135\225\222\56\106\77\167\250", "\76\58\44\213\142\177"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\233\37\1\40\72\202\54\6", "\24\171\68\114\77")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
