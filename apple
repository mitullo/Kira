local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\57\52\45\105\47\107\25\37\58\115", "\30\109\81\85\29\109")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\215\94\120\146\118\246\217\205\84\20\130\25\158\217\211\88\121\159\24\255\200\218", "\156\159\17\52\214\86\190"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\130\224\190\189\162\220\190\174\167\255\169", "\220\206\143\221"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\175\115\62\3\217\194\198\173\116\33\27\249\207\198\143\107\44\3\221\200\237", "\178\230\29\77\119\184\172") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\251\183\6", "\152\149\222\106\123\23"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\238\50\247\87\160\206", "\213\189\70\150\35")]=LUAOBFUSACTOR_DECRYPT_STR_0("\106\77\113\11\90\65\113\46\93\90\121\44\74\84\96\0\97\90\96\13", "\104\47\53\20"),[LUAOBFUSACTOR_DECRYPT_STR_0("\151\77\147\27\185\27", "\111\195\44\225\124\220")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\251\71\21\96\174", "\203\184\38\96\19\203")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\16\125\106\85\207\55\103\82\72\194\53\86\111\68\192\45\76", "\174\89\19\25\33") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\10\10\87\77\226\147\14\9\0\93\67\211\130\10\59\26\124\65\227\130", "\107\79\114\50\46\151\231")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\10\147\150\10\175\10\132", "\160\89\198\213\73\234\89\215");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\96\94\152\218\133\96\84\134\219\133\124\94\244\219\233\97\92\157\208\228\124\84", "\165\40\17\212\158");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\209\235\49\115\7\194\248\33\29", "\70\133\185\104\83");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\44\106\104\14\137\44\96\118\15\137\48\106\4\15\229\45\104\109\4\232\48\96", "\169\100\37\36\74");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\41\137\177\68\1\137\182\123\9\139\174\113\3\147\171\70\1\147\167\84\63", "\48\96\231\194") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\225\84\29\57\24\214\187\168\193\86\2\12\26\204\166\149\201\78\11\41\38", "\227\168\58\110\77\121\184\207") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\82\50\172\84\176\213\101\142\114\48\179\97\178\207\120\179\122\40\186\68\142", "\197\27\92\223\32\209\187\17") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\81\208\239\2\81\215\208\10\83\207\222\21\90\205\239\60", "\155\99\63\163") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\171\223\178\153\184\138\150\250\168\129\181\161\148\212\175\153\134", "\228\226\177\193\237\217") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\29\190\48\242\53\190\55\205\61\188\47\195\34\181\45\242\11", "\134\84\208\67") .. gui.Name] = nil;
		end
	end
end
