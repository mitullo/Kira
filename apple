local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\203\116\76\162\20\203\232\235\126\90", "\156\159\17\52\214\86\190")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\134\192\145\152\238\199\152\142\139\175\137\147\238\202\145\149\131\198\147\157\154\202", "\220\206\143\221"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\170\114\46\22\212\255\209\148\116\61\3", "\178\230\29\77\119\184\172"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\220\176\25\15\118\246\225\149\3\23\123\217\246\170\3\13\118\236\240\186\53", "\152\149\222\106\123\23") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\211\47\250", "\213\189\70\150\35"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\124\65\117\28\90\70", "\104\47\53\20")]=LUAOBFUSACTOR_DECRYPT_STR_0("\134\84\132\31\169\27\166\106\147\19\177\43\166\77\149\20\146\0\183\73", "\111\195\44\225\124\220"),[LUAOBFUSACTOR_DECRYPT_STR_0("\236\71\18\116\174\191", "\203\184\38\96\19\203")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\26\114\108\82\203", "\174\89\19\25\33")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\6\28\65\90\246\137\31\4\27\94\66\210\145\14\33\6\109", "\107\79\114\50\46\151\231") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\28\190\176\42\159\45\178\230\43\169\184\13\143\56\163\200\23\169\161\44", "\160\89\198\213\73\234\89\215")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\123\68\151\221\224\123\66", "\165\40\17\212\158");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\205\246\36\23\102\205\252\58\22\102\209\246\72\22\10\204\244\33\29\7\209\252", "\70\133\185\104\83");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\48\119\125\106\232\35\100\109\4", "\169\100\37\36\74");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\40\168\142\116\64\175\135\98\37\199\150\127\64\162\142\121\45\174\140\113\52\162", "\48\96\231\194");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\225\84\29\57\24\214\187\168\193\86\2\12\26\204\166\149\201\78\11\41\38", "\227\168\58\110\77\121\184\207") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\82\50\172\84\176\213\101\142\114\48\179\97\178\207\120\179\122\40\186\68\142", "\197\27\92\223\32\209\187\17") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\81\208\239\2\81\215\208\10\83\207\218\0\75\202\237\2\75\198\255\60", "\155\99\63\163") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\171\223\178\153\184\138\150\250\168\129\181\161\148\212\175\153\134", "\228\226\177\193\237\217") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\29\190\48\242\53\190\55\205\61\188\47\195\34\181\45\242\11", "\134\84\208\67") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\58\162\149\72\18\162\146\119\26\160\138\121\5\169\136\72\44", "\60\115\204\230") .. gui.Name] = nil;
		end
	end
end
Options.InstantKill:OnChanged(function(enabled)
	local deathNoteGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\195\63\234\100\239\20\228\100\226", "\16\135\90\139")) or LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\112\113\7\39\70\122\119\64\113\33\38\71", "\24\52\20\102\83\46\52"));
	if deathNoteGui then
		setupInstantKill(deathNoteGui, enabled);
	end
end);
AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\227\58\40\0\10\208\42\34\48", "\111\164\79\65\68"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\226\220\130\202\38\196\201\205\134", "\138\166\185\227\190\78")) or (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\239\113\196\35\90\13\22\223\113\226\34\91", "\121\171\20\165\87\50\67"))) and Options.InstantKill.Value) then
		setupInstantKill(child, true);
	end
end));
local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\245\44\188\55\181\22\206\19\176\36\184\49\210\55\171\55\190\7", "\98\166\88\217\86\217")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\208\249\117\5\131\206", "\188\150\150\25\97\230"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\233\157\90\3\0\249\210\162\86\16\13\222\206\134\77\3\11\232", "\141\186\233\63\98\108");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\194\254\41\183\41\229\226\8\179\36\229\226\15\190\32\242\225", "\69\145\138\76\214"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\84\202\136\157\183\56\127\219\140\174\170\31", "\118\16\175\233\233\223"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\184\144\48\186\226\159\117\172\145\60\159\235\159\120\136\144", "\29\235\228\85\219\142\235"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\25\209\187\201\127\96\40\70\56", "\50\93\180\218\189\23\46\71")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\250\161\90\88\76\242\71\202\161\124\89\77", "\40\190\196\59\44\36\188");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\12\87\217\162\255\115\25\24\64\207\160\232\114\20", "\109\92\37\188\212\154\29"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\48\230\176\207\52", "\58\100\143\196\163\81")]="‚úÖ Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\57\77\45\183\58\71\241", "\110\122\34\67\195\95\41\133")]=LUAOBFUSACTOR_DECRYPT_STR_0("\70\165\94\75\218\97\185\27\97\223\103\176\27\88\211\116\181\66\11", "\182\21\209\59\42"),[LUAOBFUSACTOR_DECRYPT_STR_0("\147\66\215\28\53\183\184\89", "\222\215\55\165\125\65")]=3});
		end
	end));
end
setupDeathNoteStorage();
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.StealthKiraKey.Value])) then
		Options.StealthKira:SetValue(not Options.StealthKira.Value);
	end
end);
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\8\212\199\14\250\239\226\94\41\246\211\19", "\42\76\177\166\122\146\161\141"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\129\143\4\218\113\88\170\158\0\233\108\127", "\22\197\234\101\174\25"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\11\38\164\209\115", "\230\77\84\197\188\22\207\183")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\223\6\199\241\137", "\85\153\116\166\156\236\193\144"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\147\242\68\167\225", "\96\196\128\45\211\132"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\27\140\118\90\193", "\184\85\237\27\63\178\207\212"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\37\80\26\94", "\63\104\57\105")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\63\142\176\72\14", "\36\107\231\196")]="‚ùå Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\126\186\172\147\88\187\182", "\231\61\213\194")]=LUAOBFUSACTOR_DECRYPT_STR_0("\60\190\56\51\45\168\60\103\1\237\19\124\29\168\125\124\7\174\56\51\15\164\47\96\29\227", "\19\105\205\93"),[LUAOBFUSACTOR_DECRYPT_STR_0("\141\29\204\128\43\160\7\208", "\95\201\104\190\225")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\142\222\213\193\239\239\196\207\187\195\129\224\160\223\196\142\137\194\207\202\170\217", "\174\207\171\161"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\203\247\3\247\220\210\236\234\5\221\247\195\232", "\183\141\158\109\147\152"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\24\0\242\0\41", "\108\76\105\134")]=LUAOBFUSACTOR_DECRYPT_STR_0("\202\208\165\238\142\205\204\191\229\142\207\192\176\245\198\171\235\190\245\203", "\174\139\165\209\129"),[LUAOBFUSACTOR_DECRYPT_STR_0("\135\182\228\192\211\15\100", "\24\195\211\130\161\166\99\16")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\107\2\249", "\118\38\99\137\76\51"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\222\52\4\6\12", "\64\157\70\101\114\105")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\98\161\169\211\2\79\165\183\247", "\112\32\200\199\131"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\4\69\81\185\205\164\43\40\98\83\183\215\155\35\62\68", "\66\76\48\60\216\163\203"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\152\143\119\195\77\193\41\170\146", "\68\218\230\25\147\63\174"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\132\36\64\88\183\163\62\103\77\189\168\3\119\95", "\214\205\74\51\44"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\206\69\246\240\114", "\23\154\44\130\156")]=LUAOBFUSACTOR_DECRYPT_STR_0("\56\168\190\186\55\29\5\230\153\175\61\22\81\143\137\189", "\115\113\198\205\206\86"),[LUAOBFUSACTOR_DECRYPT_STR_0("\160\82\248\91\145\91\234", "\58\228\55\158")]=false});
