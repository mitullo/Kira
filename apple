local kiraList = Tabs.KiraFeatures:AddDropdown(LUAOBFUSACTOR_DECRYPT_STR_0("\253\136\237\72\243\223\146\235", "\191\182\225\159\41"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\31\27\60\89\142", "\162\75\114\72\53\235\231")]=LUAOBFUSACTOR_DECRYPT_STR_0("\165\56\65\236\71\11\138\53\65\230\19\41\133\46\69\241", "\98\236\92\36\130\51"),[LUAOBFUSACTOR_DECRYPT_STR_0("\146\24\0\175\64\187", "\80\196\121\108\218\37\200\213")]={},[LUAOBFUSACTOR_DECRYPT_STR_0("\45\102\14\107\66", "\234\96\19\98\31\43\110")]=false,[LUAOBFUSACTOR_DECRYPT_STR_0("\34\26\84\198\185\126\159", "\235\102\127\50\167\204\18")]=nil});
local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\119\160\248\38\113\7", "\78\48\193\149\67\36"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\25\48\180\61\115\29\55\179\43\104\31\48", "\33\80\126\224\120"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\203\169\14\193\105\197", "\60\140\200\99\164"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\179\241\28\50", "\194\231\148\100\70")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\111\98\245\134\196\229\111\127\242\138\217\230", "\168\38\44\161\195\150")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\167\253\143\115\5\193", "\118\224\156\226\22\80\136\214")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\2\166", "\224\34\142\57") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\234\174\209\209\118", "\110\190\199\165\189\19\145\61")]="‚ö†Ô∏è KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\249\228\121\252\142\201\206", "\167\186\139\23\136\235")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\90\189\137\30\90\180\200\41\31\180\156\5\90\155\135\25\31\244", "\109\122\213\232")),[LUAOBFUSACTOR_DECRYPT_STR_0("\202\226\176\49\250\254\173\62", "\80\142\151\194")]=8});
end
