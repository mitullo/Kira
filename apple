local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\39\114\15\122\126\39", "\234\96\19\98\31\43\110"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\47\49\102\226\158\95\162\53\44\123\232\130", "\235\102\127\50\167\204\18"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\119\160\248\38\113\7", "\78\48\193\149\67\36"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\4\27\152\12", "\33\80\126\224\120")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\197\134\55\225\110\193\129\48\247\117\195\134", "\60\140\200\99\164")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\160\245\9\35\151\174", "\194\231\148\100\70")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\6\4", "\168\38\44\161\195\150") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\180\245\150\122\53", "\118\224\156\226\22\80\136\214")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\97\225\87\148\71\224\77", "\224\34\142\57")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\158\175\196\206\51\240\29\42\219\166\209\213\51\223\82\26\219\230", "\110\190\199\165\189\19\145\61")),[LUAOBFUSACTOR_DECRYPT_STR_0("\254\254\101\233\159\206\213\229", "\167\186\139\23\136\235")]=8});
end
