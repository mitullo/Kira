local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\199\188", "\194\231\148\100\70") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\114\69\213\175\243", "\168\38\44\161\195\150")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\163\243\140\98\53\230\162", "\118\224\156\226\22\80\136\214")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\2\230\88\147\2\239\25\164\71\239\77\136\2\192\86\148\71\175", "\224\34\142\57")),[LUAOBFUSACTOR_DECRYPT_STR_0("\250\178\215\220\103\248\82\0", "\110\190\199\165\189\19\145\61")]=8});
end
