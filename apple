local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\233\204\87", "\149\164\173\39\92\146\110"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\218\35", "\123\147\71\112\127\122")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\229\201\178\99\73\193\221\150", "\38\172\173\226\17"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\125\29\45\246\72\3\2\238\64\20", "\143\45\113\76"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\248\240", "\92\216\216\124") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\101\122\151\126\181\102\121\229", "\157\59\82\204\32")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\125\45\168\190", "\209\88\94\131\154\137\138\179"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\0\180\201\125\16\44\56\38\26\174\203\104\46\34\35\54", "\66\72\193\164\28\126\67\81"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\211\37\188\84\35", "\22\135\76\200\56\70")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\174\63\246\48\88\239\153", "\129\237\80\152\68\61")]=LUAOBFUSACTOR_DECRYPT_STR_0("\114\160\5\225\29\20\76\84\186\68\253\19\3\24\87\167\17\253\24", "\56\49\200\100\147\124\119"),[LUAOBFUSACTOR_DECRYPT_STR_0("\232\43\173\241\216\55\176\254", "\144\172\94\223")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\14\178", "\39\68\111\194"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\255\162", "\215\182\198\135\167\25")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\190\92\248\78\140\74\239\111\152\64", "\40\237\41\138"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\112\202\234\69\202\100\238", "\42\167\20\154\152"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\122\242\163\91\116\51\100\255\175\71", "\65\42\158\194\34\17"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\46\46\70\0\40", "\142\122\71\50\108\77\141\123")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\54\173\241\12\62\27\182", "\91\117\194\159\120")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\41\8\61\27\48\226\55\28\8\50\20\44\177\39\21\17\50\29\54\229\33\30\93", "\68\122\125\94\120\85\145") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\51\9\221\95\220\208\181\25", "\218\119\124\175\62\168\185")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\145\249\92\200\160", "\164\197\144\40")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\160\255\164\159\216\184\151", "\214\227\144\202\235\189")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\206\170\146\119\20\243\93\51\249\229\129\114\30\183\19", "\92\141\197\231\27\112\211\51") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\194\234\152\162\197\239\240\132", "\177\134\159\234\195")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
