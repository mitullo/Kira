local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\203\51\13\239", "\91\131\86\108\139\174\211"),LUAOBFUSACTOR_DECRYPT_STR_0("\215\46\190\3\123\244\36\172", "\61\155\75\216\119"),LUAOBFUSACTOR_DECRYPT_STR_0("\40\174\180\40\112\8\211\0", "\189\100\203\210\92\56\105"),LUAOBFUSACTOR_DECRYPT_STR_0("\3\84\251\60\3\94\234\45\61\112\239\37", "\72\79\49\157"),LUAOBFUSACTOR_DECRYPT_STR_0("\164\181\55\168\164\191\38\185\154\156\52\187", "\220\232\208\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\217\187\227\36\25\74\177\240\172\196\34\33", "\193\149\222\133\80\76\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\234\88\73\198\243\77\95\215\212\113\74\213", "\178\166\61\47"),LUAOBFUSACTOR_DECRYPT_STR_0("\201\67\239\114\222\24\244\69\252", "\94\155\42\136\26\170"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\54\33\189\144\23\39\187\128", "\213\228\95\70"),LUAOBFUSACTOR_DECRYPT_STR_0("\24\178\197\140\99\6\180\213\129\101\11\169\207", "\23\74\219\162\228"),LUAOBFUSACTOR_DECRYPT_STR_0("\11\239\65\167\47\21\233\81\170\41\21\227\65", "\91\89\134\38\207"),LUAOBFUSACTOR_DECRYPT_STR_0("\118\231\207\62\7\229\55\84\235\218\23\1\221", "\71\36\142\168\86\115\176"),LUAOBFUSACTOR_DECRYPT_STR_0("\237\168\117\183\23\139\70\89\218\179\94\186\4", "\41\191\193\18\223\99\222\54")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\158\54\215\47\184\159\41\213\57\165", "\202\203\70\167\74"),LUAOBFUSACTOR_DECRYPT_STR_0("\0\14\203\54\99\24\14\206\32\126", "\17\76\97\188\83"),LUAOBFUSACTOR_DECRYPT_STR_0("\173\50\212\54\62\140\66\167\183\40\214\35\0\130\89\183", "\195\229\71\185\87\80\227\43")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\194\253\19\85\223\225\238\20", "\143\128\156\96\48"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\154\208\227\23\39\185\195\228", "\119\216\177\144\114"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\252\57\233\71\219\29\246\80\218\38", "\34\169\73\153"),LUAOBFUSACTOR_DECRYPT_STR_0("\134\227\28\142\184\216\4\153\185\227", "\235\202\140\107"),LUAOBFUSACTOR_DECRYPT_STR_0("\36\97\57\169\231\40\254\193\62\123\59\188\217\38\229\209", "\165\108\20\84\200\137\71\151")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\88\181\56\141\74\181\57\156", "\232\26\212\75"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
local InvisibleMode = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\30\71\100\225\228\62\75\126\237\218\56\77\119", "\151\87\41\18\136"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\111\166\222\220\251", "\158\59\207\170\176")]=LUAOBFUSACTOR_DECRYPT_STR_0("\102\80\37\64\159\70\92\63\76\204\98\81\55\76", "\236\47\62\83\41"),[LUAOBFUSACTOR_DECRYPT_STR_0("\222\172\38\58\191\142\238", "\226\154\201\64\91\202")]=false});
local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\232\71\11\17\89\181\195\69\24\51\79\165", "\220\161\41\125\120\42"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\136\120\180\2\185", "\110\220\17\192")]=LUAOBFUSACTOR_DECRYPT_STR_0("\93\119\34\19\248\62\243\171\113\57\31\31\242\53\248\169\112", "\199\20\25\84\122\139\87\145"),[LUAOBFUSACTOR_DECRYPT_STR_0("\106\6\217\171", "\138\39\105\189\206\123")]=LUAOBFUSACTOR_DECRYPT_STR_0("\43\8\142\42\255\252", "\159\127\103\233\77\147\153\175"),[LUAOBFUSACTOR_DECRYPT_STR_0("\35\245\226\171\85\199\19", "\171\103\144\132\202\32")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\56\58\228\13\30\32\224\8", "\108\112\79\137"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\215\121\41\163\14\224\49\13\205\123\60\157\0\251\33", "\85\95\162\20\72\205\97\137"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\213\252\57\217\61\249\223\227", "\173\151\157\74\188\109\152")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\13\6\46\212\207\93\215\255\33\36\55\210\204", "\147\68\104\88\189\188\52\181"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\51\134\157\217\9\129\137\220\31\164\132\223\10", "\176\122\232\235")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\169\123\44\70\253\137\119\54\74\194\143\122\42", "\142\224\21\90\47")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\93\218\49\95\183\130\135\120\209\11\89\171\155", "\229\20\180\71\54\196\235")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\0\112\215\234\230\163\130\37\123\237\236\250\186", "\224\73\30\161\131\149\202")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\216\235\231\89\226\236\243\92\244\201\254\95\225", "\48\145\133\145")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\114\89\184\239\223\35\83\72", "\76\58\44\213\142\177"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\227\49\31\44\118\196\45\22\31\119\196\48\34\44\106\223", "\24\171\68\114\77"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\205\28\67\87\183\223\22\185", "\205\143\125\48\50\231\190\100")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
	end
end);
