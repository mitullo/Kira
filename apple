local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\144\35\8\14", "\178\216\70\105\106\64"),LUAOBFUSACTOR_DECRYPT_STR_0("\19\46\124\226\239\218\219\148", "\224\95\75\26\150\169\181\180"),LUAOBFUSACTOR_DECRYPT_STR_0("\39\223\222\60\108\173\120\15", "\22\107\186\184\72\36\204"),LUAOBFUSACTOR_DECRYPT_STR_0("\203\184\34\90\34\232\170\33\92\47\245\176", "\110\135\221\68\46"),LUAOBFUSACTOR_DECRYPT_STR_0("\207\51\10\255\226\188\44\230\36\32\238\201", "\91\131\86\108\139\174\211"),LUAOBFUSACTOR_DECRYPT_STR_0("\215\46\190\3\104\235\59\189\5\124\233\38", "\61\155\75\216\119"),LUAOBFUSACTOR_DECRYPT_STR_0("\40\174\180\40\109\25\205\1\185\158\57\95", "\189\100\203\210\92\56\105"),LUAOBFUSACTOR_DECRYPT_STR_0("\29\88\250\32\59\119\242\39\59", "\72\79\49\157"),LUAOBFUSACTOR_DECRYPT_STR_0("\186\185\54\180\156\152\48\178\140", "\220\232\208\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\183\226\56\56\118\174\226\187\247\17\62\87", "\193\149\222\133\80\76\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\244\84\72\218\210\113\64\197\195\79\99\215\193", "\178\166\61\47"),LUAOBFUSACTOR_DECRYPT_STR_0("\201\67\239\114\222\11\235\90\237\104\235\44\246", "\94\155\42\136\26\170"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\54\33\189\144\10\54\165\129\45\10\176\131", "\213\228\95\70")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\31\171\210\129\101\30\180\208\151\120", "\23\74\219\162\228"),LUAOBFUSACTOR_DECRYPT_STR_0("\21\233\81\170\41\13\233\84\188\52", "\91\89\134\38\207"),LUAOBFUSACTOR_DECRYPT_STR_0("\108\251\197\55\29\223\46\64\220\199\57\7\224\38\86\250", "\71\36\142\168\86\115\176")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\253\160\97\186\51\191\68\93", "\41\191\193\18\223\99\222\54"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\137\39\212\47\154\170\52\211", "\202\203\70\167\74"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\25\17\204\54\99\24\14\206\32\126", "\17\76\97\188\83"),LUAOBFUSACTOR_DECRYPT_STR_0("\169\40\206\50\34\183\68\177\150\40", "\195\229\71\185\87\80\227\43"),LUAOBFUSACTOR_DECRYPT_STR_0("\200\233\13\81\225\239\245\4\98\224\239\232\48\81\253\244", "\143\128\156\96\48")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\154\208\227\23\39\185\195\228", "\119\216\177\144\114"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
