local AllIDsESP = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\11\36\173\106\82\57\13\146\115", "\22\74\72\193\35"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\24\112\240\84\41", "\56\76\25\132")]=LUAOBFUSACTOR_DECRYPT_STR_0("\109\201\164\49\143\127\205\167\102\230\122\210\235\3\252\110", "\175\62\161\203\70"),[LUAOBFUSACTOR_DECRYPT_STR_0("\24\216\197\18\32\48\201", "\85\92\189\163\115")]=false});
