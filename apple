	local deathNoteGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\112\113\7\39\70\122\119\64\113", "\24\52\20\102\83\46\52")) or LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\224\42\32\48\7\234\32\53\33\40\209\38", "\111\164\79\65\68"));
	if deathNoteGui then
		setupInstantKill(deathNoteGui, enabled);
	end
end);
