local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\247\59\36\37\3\208\39\10\45\29\197\28\53\43\29\197\40\36", "\111\164\79\65\68")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\224\214\143\218\43\248", "\138\166\185\227\190\78"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\248\96\192\54\94\55\17\224\125\215\54\97\55\22\217\117\194\50", "\121\171\20\165\87\50\67");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\245\44\188\55\181\22\206\28\188\55\173\10\229\48\188\53\178", "\98\166\88\217\86\217"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\210\243\120\21\142\242\249\226\124\38\147\213", "\188\150\150\25\97\230"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\233\157\90\3\0\249\210\174\74\11\40\232\206\140\92\22", "\141\186\233\63\98\108"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\213\239\45\162\45\223\229\56\179", "\69\145\138\76\214")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\84\202\136\157\183\56\127\219\140\174\170\31", "\118\16\175\233\233\223");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\187\150\48\173\235\133\105\175\129\38\175\252\132\100", "\29\235\228\85\219\142\235"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\9\221\174\209\114", "\50\93\180\218\189\23\46\71")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\253\171\85\88\65\210\92", "\40\190\196\59\44\36\188")]=LUAOBFUSACTOR_DECRYPT_STR_0("\15\81\217\181\246\105\5\124\110\213\166\251\61\31\57\68\216\173\187", "\109\92\37\188\212\154\29"),[LUAOBFUSACTOR_DECRYPT_STR_0("\32\250\182\194\37\83\11\225", "\58\100\143\196\163\81")]=3});
		end
	end));
end
