local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\154\234\165\168\140\250\169\168\161\225", "\220\206\143\221")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\174\82\1\51\152\228\247\180\88\109\35\247\140\247\170\84\0\62\246\237\230\163", "\178\230\29\77\119\184\172"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\217\177\9\26\123\203\246\172\3\11\99", "\152\149\222\106\123\23"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\244\40\229\87\180\211\50\221\74\185\209\7\245\87\188\203\39\226\70\177\226", "\213\189\70\150\35") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\65\92\120", "\104\47\53\20"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\144\88\128\8\169\28", "\111\195\44\225\124\220")]=LUAOBFUSACTOR_DECRYPT_STR_0("\253\94\5\112\190\191\221\96\18\124\166\143\221\71\20\123\133\164\204\67", "\203\184\38\96\19\203"),[LUAOBFUSACTOR_DECRYPT_STR_0("\13\114\107\70\203\45", "\174\89\19\25\33")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\12\19\71\93\242", "\107\79\114\50\46\151\231")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\16\168\166\61\139\55\163\235\48\170\185\12\156\60\185\212\6", "\160\89\198\213\73\234\89\215") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\109\105\177\253\208\92\116\146\236\202\69\85\177\255\209\64\95\187\234\192", "\165\40\17\212\158")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\214\236\43\16\3\214\234", "\70\133\185\104\83");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\44\106\104\14\137\44\96\118\15\137\48\106\4\15\229\45\104\109\4\232\48\96", "\169\100\37\36\74");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\52\181\155\16\33\160\131\121\46", "\48\96\231\194");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\224\117\34\9\89\240\138\177\237\26\58\2\89\253\131\170\229\115\32\12\45\253", "\227\168\58\110\77\121\184\207");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\82\50\172\84\176\213\101\142\114\48\179\97\178\207\120\179\122\40\186\68\142", "\197\27\92\223\32\209\187\17") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\81\208\239\2\81\215\208\10\83\207\218\0\75\202\237\2\75\198\255\60", "\155\99\63\163") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\171\223\178\153\184\138\150\250\168\129\181\165\129\197\168\155\184\144\135\213\158", "\228\226\177\193\237\217") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\29\190\48\242\53\190\55\205\61\188\47\195\34\181\45\242\11", "\134\84\208\67") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\58\162\149\72\18\162\146\119\26\160\138\121\5\169\136\72\44", "\60\115\204\230") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\206\52\248\100\230\52\255\91\238\54\231\85\241\63\229\100\216", "\16\135\90\139") .. gui.Name] = nil;
		end
	end
end
