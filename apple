local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\33\30\95\194\153\91", "\235\102\127\50\167\204\18"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\121\143\193\6\118\3\121\146\198\10\107\0", "\78\48\193\149\67\36"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\31\141\29\116\25", "\33\80\126\224\120"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\216\173\27\208", "\60\140\200\99\164")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\174\218\48\3\144\170\221\55\21\139\168\218", "\194\231\148\100\70")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\97\77\204\166\195\225", "\168\38\44\161\195\150")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\192\180", "\118\224\156\226\22\80\136\214") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\118\231\77\140\71", "\224\34\142\57")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\253\168\203\201\118\255\73", "\110\190\199\165\189\19\145\61")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\154\227\118\251\203\198\154\207\114\233\159\207\154\197\120\252\142\134", "\167\186\139\23\136\235")),[LUAOBFUSACTOR_DECRYPT_STR_0("\62\160\154\12\14\188\135\3", "\109\122\213\232")]=8});
end
