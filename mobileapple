local YourIDESP = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\142\88\208\15\8\154\146\100\245", "\222\215\55\165\125\65"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\24\216\210\22\247", "\42\76\177\166\122\146\161\141")]=LUAOBFUSACTOR_DECRYPT_STR_0("\150\130\10\217\57\79\170\159\23\142\80\82\229\175\54\254", "\22\197\234\101\174\25"),[LUAOBFUSACTOR_DECRYPT_STR_0("\9\49\163\221\99\163\195", "\230\77\84\197\188\22\207\183")]=false});
local IDESPTransparency = Tabs.IDSystem:AddSlider(LUAOBFUSACTOR_DECRYPT_STR_0("\208\48\227\207\188\149\226\52\247\7\214\253\158\164\254\54\224", "\85\153\116\166\156\236\193\144"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\144\233\89\191\225", "\96\196\128\45\211\132")]=LUAOBFUSACTOR_DECRYPT_STR_0("\28\169\59\122\225\159\244\236\39\140\117\76\194\174\166\221\59\142\98", "\184\85\237\27\63\178\207\212"),[LUAOBFUSACTOR_DECRYPT_STR_0("\37\80\7", "\63\104\57\105")]=0,[LUAOBFUSACTOR_DECRYPT_STR_0("\38\134\188", "\36\107\231\196")]=0.8,[LUAOBFUSACTOR_DECRYPT_STR_0("\121\176\164\134\72\185\182", "\231\61\213\194")]=0.4,[LUAOBFUSACTOR_DECRYPT_STR_0("\59\162\40\125\13\164\51\116", "\19\105\205\93")]=2});
local function updatePrompt(prompt)
	if (not prompt or not prompt.Parent) then
		return;
	end
	if (prompt:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\153\26\209\153\54\164\1\202\152\15\187\7\211\145\43", "\95\201\104\190\225")) and (prompt.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\134\207\241\220\160\198\209\218", "\174\207\171\161"))) then
		prompt.HoldDuration = (Options.InstantTakeIDs.Value and 0) or 3;
		prompt.MaxActivationDistance = (Options.BiggerIDRange.Value and 20) or 10;
		prompt.RequiresLineOfSight = false;
	end
end
local function findAllPrompts()
	Cache.IDPrompts = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\192\255\29", "\183\141\158\109\147\152"));
	if not Map then
		return;
	end
	for _, obj in ipairs(Map:GetDescendants()) do
		if (obj:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\28\27\233\20\37\4\239\24\53\57\244\3\33\25\242", "\108\76\105\134")) and (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\194\193\129\243\193\230\213\165", "\174\139\165\209\129"))) then
			Cache.IDPrompts[obj] = true;
			updatePrompt(obj);
		end
	end
end
workspace.DescendantAdded:Connect(function(obj)
	if (obj:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\147\161\237\217\207\14\121\108\186\131\240\206\203\19\100", "\24\195\211\130\161\166\99\16")) and (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\111\7\217\62\92\27\86\23", "\118\38\99\137\76\51"))) then
		Cache.IDPrompts[obj] = true;
		updatePrompt(obj);
	end
end);
workspace.DescendantRemoving:Connect(function(obj)
	if (obj:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\205\52\10\10\0\45\244\50\28\34\27\47\240\54\17", "\64\157\70\101\114\105")) and (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\105\172\151\241\31\77\184\179", "\112\32\200\199\131"))) then
		Cache.IDPrompts[obj] = nil;
	end
end);
local function refreshIDPrompts()
	Cache.IDPrompts = {};
	findAllPrompts();
	for prompt, _ in pairs(Cache.IDPrompts) do
		if (prompt and prompt.Parent) then
			updatePrompt(prompt);
		end
	end
end
workspace.ChildRemoved:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\1\81\76", "\66\76\48\60\216\163\203")) then
		Cache.IDPrompts = {};
		for _, highlight in pairs(Cache.IDHighlights) do
			if highlight then
				highlight:Destroy();
			end
		end
		Cache.IDHighlights = {};
		for _, bb in pairs(Cache.IDBillboards) do
			if bb then
				bb:Destroy();
			end
		end
		Cache.IDBillboards = {};
		task.wait(1);
		refreshIDPrompts();
		updateIDCollectList();
	end
end);
workspace.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\151\135\105", "\68\218\230\25\147\63\174")) then
		refreshIDPrompts();
		updateIDCollectList();
	end
end);
Options.InstantTakeIDs:OnChanged(function()
	for prompt, _ in pairs(Cache.IDPrompts) do
		if (prompt and prompt.Parent) then
			updatePrompt(prompt);
		else
			Cache.IDPrompts[prompt] = nil;
		end
	end
end);
Options.BiggerIDRange:OnChanged(function()
	for prompt, _ in pairs(Cache.IDPrompts) do
		if (prompt and prompt.Parent) then
			updatePrompt(prompt);
		else
			Cache.IDPrompts[prompt] = nil;
		end
	end
end);
local function clearIDESP()
	for id, highlight in pairs(Cache.IDHighlights) do
		if (highlight and highlight.Parent) then
			highlight:Destroy();
		end
		if id:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\143\43\64\73\134\172\56\71", "\214\205\74\51\44")) then
			id.Transparency = 1;
		end
		local gui = id:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\201\89\240\250\118\249\73\197\233\126", "\23\154\44\130\156"));
		if gui then
			gui.AlwaysOnTop = false;
			gui.LightInfluence = 1;
		end
	end
	for id, bb in pairs(Cache.IDBillboards) do
		if (bb and bb.Parent) then
			bb:Destroy();
		end
	end
	Cache.IDHighlights = {};
	Cache.IDBillboards = {};
end
local function updateIDESP()
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\60\167\189", "\115\113\198\205\206\86"));
	if not Map then
		return;
	end
	local localPlayerName = LocalPlayer.Name;
	local showAll = Options.AllIDsESP.Value;
	local showMine = Options.YourIDESP.Value;
	if (not showAll and not showMine) then
		return;
	end
	local validIDs = {};
	for _, id in ipairs(Map:GetChildren()) do
		if (id.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\173\83", "\58\228\55\158")) then
			local surfaceGui = id:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\135\156\194\40\61\174\48\147\156\217", "\85\212\233\176\78\92\205"));
			local idPrompt = id:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\99\92\184\240\69\85\152\246", "\130\42\56\232"));
			if (surfaceGui and idPrompt and surfaceGui.Enabled and idPrompt.Enabled) then
				local playerName = idPrompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\218\185\37\250\69\45\196\180\41\230", "\95\138\213\68\131\32"));
				local shouldShow = showAll or (showMine and (playerName == localPlayerName));
				if (playerName and shouldShow) then
					validIDs[id] = true;
					local ownerPlayer = Players:FindFirstChild(playerName);
					local displayName = (ownerPlayer and ownerPlayer.DisplayName) or playerName;
					local isMine = playerName == localPlayerName;
					local isKira = kiraUsernames[playerName] or false;
					local color = (isMine and Color3.fromRGB(0, 255, 0)) or (isKira and Color3.fromRGB(255, 0, 0)) or Color3.fromRGB(255, 255, 255);
					local highlight = Cache.IDHighlights[id];
					if (not highlight or not highlight.Parent) then
						highlight = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\8\39\185\107\119\36\44\173\70\87\46\39\179\77\123\47\38\181", "\22\74\72\193\35"));
						highlight.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\5\93\204\81\43\113\232\81\43\113\240", "\56\76\25\132");
						highlight.Adornee = id;
						highlight.Parent = game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\125\206\185\35\232\75\200", "\175\62\161\203\70"));
						highlight.Size = id.Size;
						highlight.ZIndex = 5;
						highlight.AlwaysOnTop = true;
						Cache.IDHighlights[id] = highlight;
					end
					highlight.Color3 = color;
					highlight.Transparency = Options.IDESPTransparency.Value;
					id.Transparency = 0;
					surfaceGui.AlwaysOnTop = true;
					surfaceGui.LightInfluence = 0;
					local billboard = Cache.IDBillboards[id];
					if (not billboard or not billboard.Parent) then
						billboard = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\30\212\207\31\55\51\220\209\23\18\41\212", "\85\92\189\163\115"));
						billboard.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\0\136\18\49\37\160\50\55\40\190\52", "\88\73\204\80");
						billboard.Adornee = id;
						billboard.Size = UDim2.new(0, 200, 0, 50);
						billboard.StudsOffset = Vector3.new(0, (id.Size.Y / 2) + 2, 0);
						billboard.AlwaysOnTop = true;
						local textLabel = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\26\134\8\82\5\219\44\134\28", "\186\78\227\112\38\73"));
						textLabel.Parent = billboard;
						textLabel.Size = UDim2.new(1, 0, 1, 0);
						textLabel.BackgroundTransparency = 1;
						textLabel.Text = displayName;
						textLabel.TextColor3 = color;
						textLabel.TextScaled = false;
						textLabel.TextSize = 15;
						textLabel.Font = Enum.Font.GothamBold;
						textLabel.TextStrokeTransparency = 0.5;
						billboard.Parent = id;
						Cache.IDBillboards[id] = billboard;
					else
						local t = billboard:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\200\82\229\65\127\123\254\82\241", "\26\156\55\157\53\51"));
						if t then
							t.Text = displayName;
							t.TextColor3 = color;
						end
					end
				end
			end
		end
	end
	for id, highlight in pairs(Cache.IDHighlights) do
		if not validIDs[id] then
			if highlight then
				highlight:Destroy();
			end
			if id:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\174\217\5\220\136\81\158\204", "\48\236\184\118\185\216")) then
				id.Transparency = 1;
			end
			local gui = id:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\214\168\69\54\206\55\224\154\66\57", "\84\133\221\55\80\175"));
			if gui then
				gui.AlwaysOnTop = false;
			end
			Cache.IDHighlights[id] = nil;
			if Cache.IDBillboards[id] then
				Cache.IDBillboards[id]:Destroy();
				Cache.IDBillboards[id] = nil;
			end
		end
	end
end
local espConnection;
local function toggleIDConnection()
	local anyEnabled = Options.AllIDsESP.Value or Options.YourIDESP.Value;
	if anyEnabled then
		if not espConnection then
			espConnection = RunService.Heartbeat:Connect(function()
				local currentTime = tick();
				if ((currentTime - lastIDPromptUpdate) >= 0.5) then
					lastIDPromptUpdate = currentTime;
					updateIDESP();
				end
			end);
		end
		updateIDESP();
	else
		if espConnection then
			espConnection:Disconnect();
			espConnection = nil;
		end
		clearIDESP();
	end
end
Options.AllIDsESP:OnChanged(toggleIDConnection);
Options.YourIDESP:OnChanged(toggleIDConnection);
Options.IDESPTransparency:OnChanged(updateIDESP);
