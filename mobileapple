local kiraList = Tabs.KiraFeatures:AddDropdown(LUAOBFUSACTOR_DECRYPT_STR_0("\123\168\231\34\104\39\67\181", "\78\48\193\149\67\36"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\4\23\148\20\68", "\33\80\126\224\120")]=LUAOBFUSACTOR_DECRYPT_STR_0("\197\172\6\202\72\229\174\10\193\88\172\131\10\214\93\255", "\60\140\200\99\164"),[LUAOBFUSACTOR_DECRYPT_STR_0("\177\245\8\51\167\148", "\194\231\148\100\70")]={},[LUAOBFUSACTOR_DECRYPT_STR_0("\107\89\205\183\255", "\168\38\44\161\195\150")]=false,[LUAOBFUSACTOR_DECRYPT_STR_0("\164\249\132\119\37\228\162", "\118\224\156\226\22\80\136\214")]=nil});
local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\101\239\84\133\119\199", "\224\34\142\57"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\247\137\241\248\65\220\116\61\237\142\234\243", "\110\190\199\165\189\19\145\61"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\253\234\122\237\190\238", "\167\186\139\23\136\235"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\46\176\144\25", "\109\122\213\232")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\199\217\150\21\220\218\139\3\221\222\141\30", "\80\142\151\194")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\36\199\122\73\54\239", "\44\99\166\23")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\60\191", "\196\28\151\73\86\83") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\199\10\61\28\135", "\22\147\99\73\112\226\56\120")]="‚ö†Ô∏è KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\155\122\236\225\136\182\97", "\237\216\21\130\149")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\194\70\94\76\240\200\30\166\75\94\75\184\137\112\141\90\90\30", "\62\226\46\63\63\208\169")),[LUAOBFUSACTOR_DECRYPT_STR_0("\193\12\71\130\11\4\32\80", "\62\133\121\53\227\127\109\79")]=8});
end
