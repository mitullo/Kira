local SelectiveIDCollect = Tabs.IDSystem:AddDropdown(LUAOBFUSACTOR_DECRYPT_STR_0("\227\135\181\246\0\247\221\198\135\144\215\32\236\216\220\135\186\231", "\180\176\226\217\147\99\131"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\231\176\59\11\214", "\103\179\217\79")]=LUAOBFUSACTOR_DECRYPT_STR_0("\105\184\16\217\68\143\183\10\132\12\208\66\133\165\67\180\92\229\77\141\186\79\165\92\252\101", "\195\42\215\124\181\33\236"),[LUAOBFUSACTOR_DECRYPT_STR_0("\41\92\36\61\55\241\29\77\62\49\43", "\152\109\57\87\94\69")]=LUAOBFUSACTOR_DECRYPT_STR_0("\218\223\5\172\173\215\20\169\185\199\6\162\167\215\70\232\237\216\74\160\177\222\88\173\250\195\74\183\182\215\93\186\185\254\46", "\200\153\183\106\195\222\178\52"),[LUAOBFUSACTOR_DECRYPT_STR_0("\4\226\132\40\76\73", "\58\82\131\232\93\41")]={},[LUAOBFUSACTOR_DECRYPT_STR_0("\174\66\220\1\84", "\95\227\55\176\117\61")]=false,[LUAOBFUSACTOR_DECRYPT_STR_0("\60\123\37\74\190\20\106", "\203\120\30\67\43")]=nil});
local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\220\36\93", "\185\145\69\45\143"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\163\27", "\188\234\127\121\198")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\17\54\35\145\55\63\3\151", "\227\88\82\115"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\115\19\187\190\7\97\109\30\183\162", "\19\35\127\218\199\98"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\92\179", "\130\124\155\106") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\235\131\205\145\235\203\55\246", "\223\181\171\150\207\195\150\28")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\9\41\168\234", "\105\44\90\131\206"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\215\245\191\184\6\49\246\228\128\182\7\42\207\225\160\173", "\94\159\128\210\217\104"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\100\240\18\179\90", "\26\48\153\102\223\63\31\153")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\33\79\227\231\7\78\249", "\147\98\32\141")]=LUAOBFUSACTOR_DECRYPT_STR_0("\59\75\226\216\7\85\95\29\81\163\196\9\66\11\30\76\246\196\2", "\43\120\35\131\170\102\54"),[LUAOBFUSACTOR_DECRYPT_STR_0("\112\19\149\183\177\185\139\90", "\228\52\102\231\214\197\208")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\51\225\101", "\182\126\128\21\170\138\235\121"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\162\222", "\102\235\186\85\134\230\115\80")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\100\25\44\89\115\215\39\112\25\55", "\66\55\108\94\63\18\180"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\61\137\181\37\40\84\4\153", "\57\116\237\229\87\71"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\154\189\236\254\114\252\105\171\188\232", "\39\202\209\141\135\23\142"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\203\58\29\6\55", "\152\159\83\105\106\82")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\162\201\95\230\204\82\149", "\60\225\166\49\146\169")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\28\11\44\41\4\20\60\24\58\38\13\30\111\29\32\38\13\2\44\10\42\46\65", "\103\79\126\79\74\97") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\158\106\193\114\74\19\181\113", "\122\218\31\179\19\62")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\135\223\217\205\204", "\37\211\182\173\161\169\193")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\212\53\67\205\45\117\173", "\217\151\90\45\185\72\27")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\224\115\242\30\82\131\114\232\6\22\197\117\233\22\22", "\54\163\28\135\114") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\12\206\79\131\90\118\39\213", "\31\72\187\61\226\46")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
findAllPrompts();
toggleIDConnection();
updateIDCollectList();
local isInMatch = false;
local previousInMatch = false;
local function updateMatchState()
	local mapExists = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\7\83", "\68\163\102\35\178\39\30")) ~= nil;
	local inIntermission = false;
	local playerGui = LocalPlayer:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\142\124\219\222\6\167\164\4\183", "\113\222\16\186\167\99\213\227"));
	if playerGui then
		local gameUI = playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\15\246\243\27\39", "\150\78\110\155"));
		local topFrame = gameUI and gameUI:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\177\202\55\199\182\31\178\69", "\32\229\165\71\129\196\126\223"));
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\228\185\240\136\149\217\198", "\181\163\233\164\225\225"));
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\100\142\38\99\124\138\60\114\92", "\23\48\235\94"));
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\85\244\236\120\101\30\251\79\233\241\114\121", "\178\28\186\184\61\55\83"))) then
			inIntermission = true;
		end
	end
	previousInMatch = isInMatch;
	isInMatch = mapExists and not inIntermission;
	if (previousInMatch and not isInMatch) then
		cleanupAllESP();
	end
end
updateMatchState();
workspace.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\233\204\87", "\149\164\173\39\92\146\110")) then
		updateMatchState();
	end
end);
workspace.ChildRemoved:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\222\38\0", "\123\147\71\112\127\122")) then
		updateMatchState();
	end
end);
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\235\204\143\116\115\229", "\38\172\173\226\17")) then
		local topFrame = child:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\121\30\60\201\95\16\33\234", "\143\45\113\76"), true);
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\159\136\40\53\172\180\25", "\92\216\216\124"), true);
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\111\55\180\84\209\90\48\169\76", "\157\59\82\204\32"));
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\12\59\251\238", "\209\88\94\131\154\137\138\179")):Connect(updateMatchState);
		end
		updateMatchState();
	end
end);
task.spawn(function()
	while not LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\15\160\201\121\43\10", "\66\72\193\164\28\126\67\81")) do
		task.wait(0.5);
	end
	updateMatchState();
end);
