	if enabled then
		for _, player in pairs(Players:GetPlayers()) do
			if moderatorIDs[tostring(player.UserId)] then
				LocalPlayer:Kick("\nModerator Detected: " .. player.Name .. "\nAuto-kick enabled.");
				return;
			end
		end
	end
end);
