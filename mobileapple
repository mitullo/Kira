local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\5\93\246\17\127\218\62\98\250\2\114\253\34\70\225\17\116\203", "\174\86\41\147\112\19")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\125\15\129\15\32\29", "\203\59\96\237\107\69\111\113"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\23\2\169\224\61\228\223\15\31\190\224\2\228\216\54\23\171\228", "\183\68\118\204\129\81\144");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\61\185\117\229\7\150\6\137\117\229\31\138\45\165\117\231\0", "\226\110\205\16\132\107"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\207\198\225\205\73\197\204\244\220\102\254\202", "\33\139\163\128\185"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\100\76\1\223\91\76\12\249\66\81\32\219\67\93\7\202", "\190\55\56\100"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\114\170\61\10\27\205\252\66\170", "\147\54\207\92\126\115\131")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\41\52\52\105\5\80\2\37\48\90\24\119", "\30\109\81\85\29\109");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\207\99\81\160\51\208\232\219\116\71\162\36\209\229", "\156\159\17\52\214\86\190"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\154\230\169\176\171", "\220\206\143\221")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\165\114\35\3\221\194\198", "\178\230\29\77\119\184\172")]=LUAOBFUSACTOR_DECRYPT_STR_0("\198\170\15\26\123\236\253\254\33\18\101\249\181\172\15\26\115\225\180", "\152\149\222\106\123\23"),[LUAOBFUSACTOR_DECRYPT_STR_0("\249\51\228\66\161\212\41\248", "\213\189\70\150\35")]=3});
		end
	end));
end
setupDeathNoteStorage();
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\107\80\117\28\71\123\123\28\74\114\97\1", "\104\47\53\20"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\135\73\128\8\180\33\172\88\132\59\169\6", "\111\195\44\225\124\220"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\254\84\1\126\174", "\203\184\38\96\19\203")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\31\97\120\76\203", "\174\89\19\25\33"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\24\0\91\90\242", "\107\79\114\50\46\151\231"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\167\184\44\153", "\160\89\198\213\73\234\89\215"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\101\120\167\255", "\165\40\17\212\158")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\209\208\28\63\35", "\70\133\185\104\83")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\39\74\74\62\204\10\81", "\169\100\37\36\74")]=LUAOBFUSACTOR_DECRYPT_STR_0("\53\148\167\16\36\130\163\68\8\199\140\95\20\130\226\95\14\132\167\16\6\142\176\67\20\201", "\48\96\231\194"),[LUAOBFUSACTOR_DECRYPT_STR_0("\236\79\28\44\13\209\160\141", "\227\168\58\110\77\121\184\207")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\90\41\171\79\241\255\116\164\111\52\255\110\190\207\116\229\93\53\177\68\180\201", "\197\27\92\223\32\209\187\17"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\37\86\205\255\39\90\194\239\11\113\204\239\6", "\155\99\63\163"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\182\216\181\129\188", "\228\226\177\193\237\217")]=LUAOBFUSACTOR_DECRYPT_STR_0("\21\165\55\233\116\150\42\232\48\240\7\227\53\164\43\166\26\191\55\227", "\134\84\208\67"),[LUAOBFUSACTOR_DECRYPT_STR_0("\55\169\128\93\6\160\146", "\60\115\204\230")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\202\59\251", "\16\135\90\139"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\119\102\7\39\75", "\24\52\20\102\83\46\52")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\230\38\47\20\29\203\34\49\48", "\111\164\79\65\68"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\204\142\223\32\229\207\221\177\209\33\254\246\216\145\202", "\138\166\185\227\190\78"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\233\125\203\7\64\44\20\219\96", "\121\171\20\165\87\50\67"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\239\54\170\34\184\12\210\12\184\61\188\43\226\43", "\98\166\88\217\86\217"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\194\255\109\13\131", "\188\150\150\25\97\230")]=LUAOBFUSACTOR_DECRYPT_STR_0("\243\135\76\22\13\227\206\201\107\3\7\232\154\160\123\17", "\141\186\233\63\98\108"),[LUAOBFUSACTOR_DECRYPT_STR_0("\213\239\42\183\48\253\254", "\69\145\138\76\214")]=false});
