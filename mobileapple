local StealthKira = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\18\85\85\5\251\72\43\10\72\66\5", "\67\65\33\48\100\151\60"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\235\238\186\212\246", "\147\191\135\206\184")]=LUAOBFUSACTOR_DECRYPT_STR_0("\183\60\163\192\212\71\186\196\3\175\211\217", "\210\228\72\198\161\184\51"),[LUAOBFUSACTOR_DECRYPT_STR_0("\18\76\224\19\97\199\38\93\250\31\125", "\174\86\41\147\112\19")]="‚ö†Ô∏è USE DEATH NOTE ONCE BEFORE ENABLING",[LUAOBFUSACTOR_DECRYPT_STR_0("\127\5\139\10\48\3\5", "\203\59\96\237\107\69\111\113")]=false});
local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\2\169\224\61\228\223\15\31\190\224\2\228\216\54\23\171\228", "\183\68\118\204\129\81\144")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\40\162\124\224\14\144", "\226\110\205\16\132\107"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\216\215\229\216\77\255\203\203\208\83\234\240\244\214\83\234\196\229", "\33\139\163\128\185");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\100\76\1\223\91\76\12\250\82\89\16\214\116\80\1\221\92", "\190\55\56\100"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\114\170\61\10\27\205\252\66\170\27\11\26", "\147\54\207\92\126\115\131"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\62\37\48\124\1\106\5\22\32\116\41\123\25\52\54\105", "\30\109\81\85\29\109"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\219\116\85\162\62\240\243\235\116", "\156\159\17\52\214\86\190")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\138\234\188\168\166\193\178\168\171\200\168\181", "\220\206\143\221");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\182\111\40\1\221\194\198\162\120\62\3\202\195\203", "\178\230\29\77\119\184\172"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\193\183\30\23\114", "\152\149\222\106\123\23")]="‚úÖ Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\254\41\248\87\176\211\50", "\213\189\70\150\35")]=LUAOBFUSACTOR_DECRYPT_STR_0("\124\65\113\9\67\65\124\72\100\92\102\9\15\71\113\9\75\76\53", "\104\47\53\20"),[LUAOBFUSACTOR_DECRYPT_STR_0("\135\89\147\29\168\6\172\66", "\111\195\44\225\124\220")]=3});
		end
	end));
end
setupDeathNoteStorage();
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\252\67\1\103\163\133\215\82\5\84\190\162", "\203\184\38\96\19\203"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\29\118\120\85\198\23\124\109\68\233\44\122", "\174\89\19\25\33"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\0\83\67\242", "\107\79\114\50\46\151\231")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\31\180\180\36\143", "\160\89\198\213\73\234\89\215"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\127\99\189\234\192", "\165\40\17\212\158"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\203\216\5\54\53", "\70\133\185\104\83"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\41\76\87\43", "\169\100\37\36\74")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\52\142\182\92\5", "\48\96\231\194")]="‚ùå Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\235\85\0\57\28\214\187", "\227\168\58\110\77\121\184\207")]=LUAOBFUSACTOR_DECRYPT_STR_0("\78\47\186\0\149\222\112\177\115\124\145\79\165\222\49\170\117\63\186\0\183\210\99\182\111\114", "\197\27\92\223\32\209\187\17"),[LUAOBFUSACTOR_DECRYPT_STR_0("\39\74\209\250\23\86\204\245", "\155\99\63\163")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\163\196\181\130\249\160\135\208\181\133\249\170\141\197\164\205\159\141\140\213\164\159", "\228\226\177\193\237\217"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\18\185\45\226\16\181\34\242\60\158\44\242\49", "\134\84\208\67"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\39\165\146\80\22", "\60\115\204\230")]=LUAOBFUSACTOR_DECRYPT_STR_0("\198\47\255\127\167\28\226\126\227\122\207\117\230\46\227\48\201\53\255\117", "\16\135\90\139"),[LUAOBFUSACTOR_DECRYPT_STR_0("\112\113\0\50\91\88\108", "\24\52\20\102\83\46\52")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\233\46\49", "\111\164\79\65\68"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\229\203\130\202\43", "\138\166\185\227\190\78")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\233\125\203\7\64\44\20\219\96", "\121\171\20\165\87\50\67"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\45\180\55\183\13\207\60\139\57\182\22\246\57\171\34", "\98\166\88\217\86\217"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\212\255\119\49\148\211\251\230\109", "\188\150\150\25\97\230"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\243\135\76\22\13\227\206\189\94\9\9\196\254\154", "\141\186\233\63\98\108"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\197\227\56\186\32", "\69\145\138\76\214")]=LUAOBFUSACTOR_DECRYPT_STR_0("\89\193\154\157\190\24\100\143\189\136\180\19\48\230\173\154", "\118\16\175\233\233\223"),[LUAOBFUSACTOR_DECRYPT_STR_0("\175\129\51\186\251\135\105", "\29\235\228\85\219\142\235")]=false});
