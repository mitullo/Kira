local kiraList = Tabs.KiraFeatures:AddDropdown(LUAOBFUSACTOR_DECRYPT_STR_0("\43\122\16\126\103\7\153\20", "\234\96\19\98\31\43\110"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\50\22\70\203\169", "\235\102\127\50\167\204\18")]=LUAOBFUSACTOR_DECRYPT_STR_0("\121\165\240\45\80\39\86\168\240\39\4\5\89\179\244\48", "\78\48\193\149\67\36"),[LUAOBFUSACTOR_DECRYPT_STR_0("\6\31\140\13\68\35", "\33\80\126\224\120")]={},[LUAOBFUSACTOR_DECRYPT_STR_0("\193\189\15\208\85", "\60\140\200\99\164")]=false,[LUAOBFUSACTOR_DECRYPT_STR_0("\163\241\2\39\183\139\224", "\194\231\148\100\70")]=nil});
local kiraUsernames = {};
local function clearKiraList()
	kiraList:SetValues({});
	kiraUsernames = {};
end
local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\97\77\204\166\195\225", "\168\38\44\161\195\150"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\169\210\182\83\2\197\159\37\179\213\173\88", "\118\224\156\226\22\80\136\214"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\101\239\84\133\119\199", "\224\34\142\57"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\234\162\221\201", "\110\190\199\165\189\19\145\61")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\243\197\67\205\185\234\243\216\68\193\164\233", "\167\186\139\23\136\235")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\61\180\133\8\47\156", "\109\122\213\232")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\174\191", "\80\142\151\194") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\55\207\99\64\6", "\44\99\166\23")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\95\248\39\34\54\170\104", "\196\28\151\73\86\83")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\179\11\40\3\194\89\88\82\246\2\61\24\194\118\23\98\246\66", "\22\147\99\73\112\226\56\120")),[LUAOBFUSACTOR_DECRYPT_STR_0("\156\96\240\244\153\177\122\236", "\237\216\21\130\149")]=8});
end
