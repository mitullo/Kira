local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\242\163\116\70", "\34\186\198\21"),LUAOBFUSACTOR_DECRYPT_STR_0("\212\13\195\73\228\247\7\209", "\162\152\104\165\61"),LUAOBFUSACTOR_DECRYPT_STR_0("\225\42\180\105\88\228\195\43", "\133\173\79\210\29\16"),LUAOBFUSACTOR_DECRYPT_STR_0("\161\121\235\63\161\115\250\46\159\93\255\38", "\75\237\28\141"),LUAOBFUSACTOR_DECRYPT_STR_0("\240\90\202\165\3\20\240\228\206\115\201\182", "\129\188\63\172\209\79\123\135"),LUAOBFUSACTOR_DECRYPT_STR_0("\108\225\224\217\117\244\246\200\82\197\244\192", "\173\32\132\134"),LUAOBFUSACTOR_DECRYPT_STR_0("\98\30\14\251\155\33\221\75\9\36\234\169", "\173\46\123\104\143\206\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\134\20\37\130\81\165\14\187\9", "\97\212\125\66\234\37\227"),LUAOBFUSACTOR_DECRYPT_STR_0("\184\234\177\61\10\162\226\184\49", "\126\234\131\214\85"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\220\78\82\91\168\218\94\95\93\165\199\68", "\47\228\181\41\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\148\245\222\51\23\28\16\177\249\203\23\6\55", "\127\198\156\185\91\99\80"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\19\203\248\179\62\41\206\240\8\237\226\170", "\190\149\122\172\144\199\107\89"),LUAOBFUSACTOR_DECRYPT_STR_0("\0\12\246\246\234\7\21\225\251\236\30\0\246", "\158\82\101\145\158")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\69\238\18\19\86\68\241\16\5\75", "\36\16\158\98\118"),LUAOBFUSACTOR_DECRYPT_STR_0("\236\25\212\254\74\220\40\247\211\25", "\133\160\118\163\155\56\136\71"),LUAOBFUSACTOR_DECRYPT_STR_0("\222\183\124\243\184\16\188\242\144\126\253\162\47\180\228\182", "\213\150\194\17\146\214\127")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\57\168\183\209\118\165\176\34", "\86\123\201\196\180\38\196\194"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\213\233\202\170\199\233\203\187", "\207\151\136\185"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\157\147\56\135\102\76\126\186\144\39", "\17\200\227\72\226\20\24"),LUAOBFUSACTOR_DECRYPT_STR_0("\156\78\12\210\219\197\224\237\163\78", "\159\208\33\123\183\169\145\143"),LUAOBFUSACTOR_DECRYPT_STR_0("\218\79\53\55\252\85\49\50\192\85\55\34\194\91\42\34", "\86\146\58\88")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\122\222\249\197\158\232\36\238", "\154\56\191\138\160\206\137\86"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
