local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\229\42\179\121", "\133\173\79\210\29\16"),LUAOBFUSACTOR_DECRYPT_STR_0("\161\121\235\63\171\115\226\63", "\75\237\28\141"),LUAOBFUSACTOR_DECRYPT_STR_0("\240\90\202\165\7\26\233\229", "\129\188\63\172\209\79\123\135"),LUAOBFUSACTOR_DECRYPT_STR_0("\108\225\224\217\108\235\241\200\82\197\244\192", "\173\32\132\134"),LUAOBFUSACTOR_DECRYPT_STR_0("\98\30\14\251\130\62\218\75\9\36\234\169", "\173\46\123\104\143\206\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\152\24\36\158\112\147\17\177\15\3\152\72", "\97\212\125\66\234\37\227"),LUAOBFUSACTOR_DECRYPT_STR_0("\166\230\176\33\43\154\243\179\39\50\143\228", "\126\234\131\214\85"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\220\78\82\91\162\218\70\78", "\47\228\181\41\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\148\245\222\51\23\24\30\168\248", "\127\198\156\185\91\99\80"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\19\203\248\179\39\54\201\240\8\237\226\170", "\190\149\122\172\144\199\107\89"),LUAOBFUSACTOR_DECRYPT_STR_0("\0\12\246\246\234\30\10\230\251\236\30\0\246", "\158\82\101\145\158"),LUAOBFUSACTOR_DECRYPT_STR_0("\66\247\5\30\80\69\238\18\19\86\81\236\15", "\36\16\158\98\118"),LUAOBFUSACTOR_DECRYPT_STR_0("\242\31\196\243\76\221\55\245\197\4\239\254\95", "\133\160\118\163\155\56\136\71")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\195\178\97\247\164\43\186\228\177\126", "\213\150\194\17\146\214\127"),LUAOBFUSACTOR_DECRYPT_STR_0("\55\166\179\209\84\144\173\36\8\166", "\86\123\201\196\180\38\196\194"),LUAOBFUSACTOR_DECRYPT_STR_0("\223\253\212\174\249\231\208\171\197\231\214\187\199\233\203\187", "\207\151\136\185")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\138\130\59\135\68\121\99\188", "\17\200\227\72\226\20\24"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\146\64\8\210\249\240\253\235", "\159\208\33\123\183\169\145\143"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\199\74\40\51\224\110\55\36\225\85", "\86\146\58\88"),LUAOBFUSACTOR_DECRYPT_STR_0("\116\208\253\197\188\221\57\232\75\208", "\154\56\191\138\160\206\137\86"),LUAOBFUSACTOR_DECRYPT_STR_0("\174\76\248\134\114\53\136\200\180\86\250\147\76\59\147\216", "\172\230\57\149\231\28\90\225")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\32\171\149\215\24\218\16\190", "\187\98\202\230\178\72"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
