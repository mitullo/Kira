local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\21\51\3", "\227\88\82\115"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\106\27", "\19\35\127\218\199\98")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\53\255\58\240\19\246\26\246", "\130\124\155\106"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\229\199\247\182\166\228\82\190\216\206", "\223\181\171\150\207\195\150\28"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\12\114", "\105\44\90\131\206") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\193\168\137\135\64\3\180\169", "\94\159\128\210\217\104")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\21\234\77\251", "\26\48\153\102\223\63\31\153"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\42\85\224\242\12\79\228\247\48\79\226\231\50\65\255\231", "\147\98\32\141"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\44\74\247\198\3", "\43\120\35\131\170\102\54")]="‚ùå Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\119\9\137\162\160\190\144", "\228\52\102\231\214\197\208")]=LUAOBFUSACTOR_DECRYPT_STR_0("\61\232\116\216\235\136\13\211\12\160\123\197\254\203\31\217\11\238\113", "\182\126\128\21\170\138\235\121"),[LUAOBFUSACTOR_DECRYPT_STR_0("\175\207\39\231\146\26\63\8", "\102\235\186\85\134\230\115\80")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\122\13\46", "\66\55\108\94\63\18\180"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\61\137", "\57\116\237\229\87\71")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\153\164\255\225\118\237\66\141\164\228", "\39\202\209\141\135\23\142"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\214\55\57\24\61\245\239\39", "\152\159\83\105\106\82"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\177\202\80\235\204\78\175\199\92\247", "\60\225\166\49\146\169"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\27\23\59\38\4", "\103\79\126\79\74\97")]="‚úÖ ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\153\112\221\103\91\20\174", "\122\218\31\179\19\62")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\128\195\206\194\204\178\86\181\195\193\205\208\225\70\188\218\193\196\202\181\64\183\150", "\37\211\182\173\161\169\193") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\211\47\95\216\60\114\182\249", "\217\151\90\45\185\72\27")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\247\117\243\30\83", "\54\163\28\135\114")]="‚ùå ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\11\212\83\150\75\113\60", "\31\72\187\61\226\46")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\224\9\86\222\67\62\42\204\18\3\212\78\112\32\131", "\68\163\102\35\178\39\30") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\154\101\200\198\23\188\140\31", "\113\222\16\186\167\99\213\227")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
findAllPrompts();
