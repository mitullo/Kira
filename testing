local FullBright = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\42\97\56\164\203\53\254\194\4\96", "\165\108\20\84\200\137\71\151"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\78\189\63\132\127", "\232\26\212\75")]=LUAOBFUSACTOR_DECRYPT_STR_0("\17\92\126\228\183\21\91\123\239\255\35", "\151\87\41\18\136"),[LUAOBFUSACTOR_DECRYPT_STR_0("\127\170\204\209\235\87\187", "\158\59\207\170\176")]=false});
local defaultLighting = {[LUAOBFUSACTOR_DECRYPT_STR_0("\110\83\49\64\137\65\74", "\236\47\62\83\41")]=Lighting.Ambient,[LUAOBFUSACTOR_DECRYPT_STR_0("\213\188\52\63\165\141\232\136\45\57\163\135\244\189", "\226\154\201\64\91\202")]=Lighting.OutdoorAmbient,[LUAOBFUSACTOR_DECRYPT_STR_0("\227\91\20\31\66\168\207\76\14\11", "\220\161\41\125\120\42")]=Lighting.Brightness};
