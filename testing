local JumpPower = Tabs.Miscellaneous:AddSlider(LUAOBFUSACTOR_DECRYPT_STR_0("\19\243\75\191\11\54\241\67\189", "\91\89\134\38\207"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\112\231\220\58\22", "\71\36\142\168\86\115\176")]=LUAOBFUSACTOR_DECRYPT_STR_0("\245\180\127\175\67\142\89\94\218\179", "\41\191\193\18\223\99\222\54"),[LUAOBFUSACTOR_DECRYPT_STR_0("\134\47\201", "\202\203\70\167\74")]=50,[LUAOBFUSACTOR_DECRYPT_STR_0("\1\0\196", "\17\76\97\188\83")]=300,[LUAOBFUSACTOR_DECRYPT_STR_0("\161\34\223\54\37\143\95", "\195\229\71\185\87\80\227\43")]=50,[LUAOBFUSACTOR_DECRYPT_STR_0("\210\243\21\94\235\233\242\7", "\143\128\156\96\48")]=0});
