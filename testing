local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\35\0\55\244\218\186\170\59\29\32\244\229\186\173\2\21\53\240", "\194\112\116\82\149\182\206")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\31\167\64\28\197\240", "\110\89\200\44\120\160\130"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\152\215\78\71\79\94\51\102\162\209\74\117\87\69\41\76\172\198", "\45\203\163\43\38\35\42\91");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\225\145\217\34\139\189\92\246\128\221\55\143\138\92\215\134\215", "\52\178\229\188\67\231\201"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\5\68\81\16\255\114\44\53\68\119\17\254", "\67\65\33\48\100\151\60"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\236\243\171\217\255\203\239\137\205\250\251\226\186\221\240\203", "\147\191\135\206\184"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\160\45\167\213\208\125\189\144\45", "\210\228\72\198\161\184\51")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\18\76\242\4\123\224\57\93\246\55\102\199", "\174\86\41\147\112\19");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\107\18\136\29\32\1\5\143\94\19\153\25\42\22", "\203\59\96\237\107\69\111\113"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\16\31\184\237\52", "\183\68\118\204\129\81\144")]="‚úÖ Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\45\162\126\240\14\140\26", "\226\110\205\16\132\107")]=LUAOBFUSACTOR_DECRYPT_STR_0("\216\215\229\216\77\255\203\160\242\72\249\194\160\203\68\234\199\249\152", "\33\139\163\128\185"),[LUAOBFUSACTOR_DECRYPT_STR_0("\115\77\22\223\67\81\11\208", "\190\55\56\100")]=3});
		end
	end));
end
setupDeathNoteStorage();
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.StealthKiraKey.Value])) then
		Options.StealthKira:SetValue(not Options.StealthKira.Value);
	end
end);
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\114\170\61\10\27\205\252\66\170\27\11\26", "\147\54\207\92\126\115\131"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\41\52\52\105\5\80\2\37\48\90\24\119", "\30\109\81\85\29\109"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\217\99\85\187\51", "\156\159\17\52\214\86\190")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\136\253\188\177\171", "\220\206\143\221"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\177\111\36\3\221", "\178\230\29\77\119\184\172"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\219\191\7\30\100", "\152\149\222\106\123\23"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\240\47\229\66", "\213\189\70\150\35")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\123\92\96\4\74", "\104\47\53\20")]="‚ùå Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\128\67\143\8\185\1\183", "\111\195\44\225\124\220")]=LUAOBFUSACTOR_DECRYPT_STR_0("\237\85\5\51\143\174\217\82\8\51\133\164\204\67\64\124\165\168\221\6\6\122\185\184\204\8", "\203\184\38\96\19\203"),[LUAOBFUSACTOR_DECRYPT_STR_0("\29\102\107\64\218\48\124\119", "\174\89\19\25\33")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\14\7\70\65\183\163\14\46\6\90\14\217\136\31\42\82\116\71\249\131\14\61", "\107\79\114\50\46\151\231"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\31\175\187\45\174\60\182\212\49\136\186\61\143", "\160\89\198\213\73\234\89\215"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\124\120\160\242\192", "\165\40\17\212\158")]=LUAOBFUSACTOR_DECRYPT_STR_0("\196\204\28\60\102\195\208\6\55\102\193\220\9\39\46\165\247\7\39\35", "\70\133\185\104\83"),[LUAOBFUSACTOR_DECRYPT_STR_0("\32\64\66\43\220\8\81", "\169\100\37\36\74")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\45\134\178", "\48\96\231\194"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\235\72\15\57\28", "\227\168\58\110\77\121\184\207")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\89\53\177\112\163\212\124\181\111", "\197\27\92\223\32\209\187\17"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\43\74\206\250\13\80\202\255\49\80\204\239\51\94\209\239", "\155\99\63\163"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\160\216\175\189\171\139\143\193\181", "\228\226\177\193\237\217"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);
local InstantTakeIDs = Tabs.IDSystem:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\29\190\48\242\53\190\55\210\53\187\38\207\16\163", "\134\84\208\67"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\39\165\146\80\22", "\60\115\204\230")]=LUAOBFUSACTOR_DECRYPT_STR_0("\206\52\248\100\230\52\255\48\211\59\224\117\167\19\207\99", "\16\135\90\139"),[LUAOBFUSACTOR_DECRYPT_STR_0("\112\113\0\50\91\88\108", "\24\52\20\102\83\46\52")]=false});
