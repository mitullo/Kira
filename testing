local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\43\68\75\211\4\14\0\226\7\97\88\195", "\142\98\42\61\186\119\103\98"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\12\182\22\4\61", "\104\88\223\98")]=LUAOBFUSACTOR_DECRYPT_STR_0("\109\249\244\199\17\228\70\251\231\142\41\232\93\245\235\192\6", "\141\36\151\130\174\98"),[LUAOBFUSACTOR_DECRYPT_STR_0("\169\117\198\8", "\109\228\26\162")]=LUAOBFUSACTOR_DECRYPT_STR_0("\106\234\250\127\236\227", "\134\62\133\157\24\128"),[LUAOBFUSACTOR_DECRYPT_STR_0("\35\160\28\216\58\189\194", "\182\103\197\122\185\79\209")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\219\146\236\118\14\71\250\131", "\40\147\231\129\23\96"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\93\237\129\68\181\163\213\113\202\131\74\175\156\221\103\236", "\188\21\152\236\37\219\204"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\98\232\36\9\112\232\37\24", "\108\32\137\87")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\131\230\22\175\60\240\73\85\175\196\15\169\63", "\57\202\136\96\198\79\153\43"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\130\45\188\174\158\174\250\167\38\134\168\130\183", "\152\203\67\202\199\237\199")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\211\77\182\6\12\124\123\234\255\111\175\0\15", "\134\154\35\192\111\127\21\25")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\145\40\31\3\51\219\186\42\12\38\47\221\168", "\178\216\70\105\106\64")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\22\37\108\255\218\220\214\140\58\7\117\249\217", "\224\95\75\26\150\169\181\180")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\34\212\206\33\87\165\116\7\223\244\39\75\188", "\22\107\186\184\72\36\204")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\207\168\41\79\0\232\180\32", "\110\135\221\68\46"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\203\35\1\234\192\188\50\231\4\3\228\218\131\58\241\34", "\91\131\86\108\139\174\211"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\217\42\171\18\109\250\57\172", "\61\155\75\216\119")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
	end
end);
