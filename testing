local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\97\123\89\11\6\111", "\83\38\26\52\110"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\113\57\19\99\106\58\14\117\107\62\8\104", "\38\56\119\71"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\212\238\85\211\16\127", "\54\147\143\56\182\69"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\226\132\231\93", "\191\182\225\159\41")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\2\60\28\112\185\170\235\24\33\1\122\165", "\162\75\114\72\53\235\231")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\171\61\73\231\102\43", "\98\236\92\36\130\51")) then
		setupIntermissionListener();
	end
end);
local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\228\81", "\80\196\121\108\218\37\200\213") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\52\122\22\115\78", "\234\96\19\98\31\43\110")]="‚ö†Ô∏è KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\37\16\92\211\169\124\159", "\235\102\127\50\167\204\18")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\16\169\244\48\4\47\16\133\240\34\80\38\16\143\250\55\65\111", "\78\48\193\149\67\36")),[LUAOBFUSACTOR_DECRYPT_STR_0("\20\11\146\25\85\57\17\142", "\33\80\126\224\120")]=8});
end
