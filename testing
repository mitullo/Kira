local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\80\250\28\65", "\37\24\159\125"),LUAOBFUSACTOR_DECRYPT_STR_0("\246\163\115\86\252\169\122\86", "\34\186\198\21"),LUAOBFUSACTOR_DECRYPT_STR_0("\212\13\195\73\234\249\6\193", "\162\152\104\165\61"),LUAOBFUSACTOR_DECRYPT_STR_0("\225\42\180\105\92\234\218\42\160\92\98\232", "\133\173\79\210\29\16"),LUAOBFUSACTOR_DECRYPT_STR_0("\161\121\235\63\161\115\250\46\159\80\232\44", "\75\237\28\141"),LUAOBFUSACTOR_DECRYPT_STR_0("\240\90\202\165\26\11\247\228\206\126\222\188", "\129\188\63\172\209\79\123\135"),LUAOBFUSACTOR_DECRYPT_STR_0("\108\225\224\217\117\244\246\200\82\200\227\202", "\173\32\132\134"),LUAOBFUSACTOR_DECRYPT_STR_0("\124\18\15\231\186\23\194\65\15", "\173\46\123\104\143\206\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\134\20\37\130\81\171\0\186\25", "\97\212\125\66\234\37\227"),LUAOBFUSACTOR_DECRYPT_STR_0("\184\234\177\61\10\166\236\161\48\12\171\241\187", "\126\234\131\214\85"),LUAOBFUSACTOR_DECRYPT_STR_0("\182\220\78\82\91\168\218\94\95\93\168\208\78", "\47\228\181\41\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\148\245\222\51\23\5\15\182\249\203\26\17\61", "\127\198\156\185\91\99\80"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\19\203\248\179\62\41\206\240\8\224\245\160", "\190\149\122\172\144\199\107\89")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\7\21\225\251\236\6\10\227\237\241", "\158\82\101\145\158"),LUAOBFUSACTOR_DECRYPT_STR_0("\92\241\21\19\86\68\241\16\5\75", "\36\16\158\98\118"),LUAOBFUSACTOR_DECRYPT_STR_0("\232\3\206\250\86\231\46\225\242\25\204\239\104\233\53\241", "\133\160\118\163\155\56\136\71")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\212\163\98\247\134\30\167\226", "\213\150\194\17\146\214\127"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\57\168\183\209\118\165\176\34", "\86\123\201\196\180\38\196\194"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\194\248\201\170\229\220\214\189\228\231", "\207\151\136\185"),LUAOBFUSACTOR_DECRYPT_STR_0("\132\140\63\135\102\76\126\186\144\39", "\17\200\227\72\226\20\24"),LUAOBFUSACTOR_DECRYPT_STR_0("\152\84\22\214\199\254\230\251\130\78\20\195\249\240\253\235", "\159\208\33\123\183\169\145\143")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\208\91\43\51\194\91\42\34", "\86\146\58\88"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
