local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\254\184\63", "\103\179\217\79"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\99\179", "\195\42\215\124\181\33\236")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\36\93\7\44\42\245\29\77", "\152\109\57\87\94\69"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\201\219\11\186\187\192\122\169\244\210", "\200\153\183\106\195\222\178\52"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\114\171", "\58\82\131\232\93\41") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\189\31\235\43\21\2\200\30", "\95\227\55\176\117\61")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\93\109\104\15", "\203\120\30\67\43"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\217\48\64\238\215\254\44\73\221\214\254\49\125\238\203\229", "\185\145\69\45\143"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\190\22\13\170\217", "\188\234\127\121\198")]="‚ùå Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\27\61\29\151\61\60\7", "\227\88\82\115")]=LUAOBFUSACTOR_DECRYPT_STR_0("\96\23\187\181\3\112\87\26\168\231\12\124\87\95\188\168\23\125\71", "\19\35\127\218\199\98"),[LUAOBFUSACTOR_DECRYPT_STR_0("\56\238\24\227\8\242\5\236", "\130\124\155\106")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\248\202\230", "\223\181\171\150\207\195\150\28"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\101\62", "\105\44\90\131\206")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\204\245\160\191\9\61\250\199\167\176", "\94\159\128\210\217\104"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\121\253\54\173\80\114\233\110", "\26\48\153\102\223\63\31\153"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\50\76\236\234\7\82\195\242\15\69", "\147\98\32\141"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\44\74\247\198\3", "\43\120\35\131\170\102\54")]="‚úÖ ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\119\9\137\162\160\190\144", "\228\52\102\231\214\197\208")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\45\245\118\201\239\152\10\208\11\236\121\211\170\136\22\218\18\229\118\222\239\143\89", "\182\126\128\21\170\138\235\121") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\175\207\39\231\146\26\63\8", "\102\235\186\85\134\230\115\80")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\99\5\42\83\119", "\66\55\108\94\63\18\180")]="‚ùå ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\55\130\139\35\34\87\0", "\57\116\237\229\87\71")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\137\190\248\235\115\174\73\165\165\173\225\126\224\67\234", "\39\202\209\141\135\23\142") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\219\38\27\11\38\241\240\61", "\152\159\83\105\106\82")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);
